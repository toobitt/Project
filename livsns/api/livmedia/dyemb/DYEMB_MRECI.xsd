			</xs:element>
			<xs:element name="FileSize" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>文件大小（字节）</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FileLength" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>文件长度，视频用帧；音频用采样点</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="tns2:ExtendAttributes" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddEMBTaskResponseType">
		<xs:sequence>
			<xs:element ref="tns:CommonResponse"/>
