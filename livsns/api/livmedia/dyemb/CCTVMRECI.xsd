				<xs:annotation>
					<xs:documentation>任务名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExecuteMode" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>任务执行模式 0：自适应1：单任务 2：分布式任务 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BusinessType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>业务类型</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceFiles">
				<xs:annotation>
					<xs:documentation>源文件信息</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="tns2:FileGroup" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TargetFiles" minOccurs="0">
				<xs:annotation>
					<xs:documentation>目标文件信息</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="tns2:FileGroup" maxOccurs="unbounded"/>
