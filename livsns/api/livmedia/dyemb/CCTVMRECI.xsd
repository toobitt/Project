			<xs:element name="EMBTaskID" type="xs:string" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>任务ID为添加任务时的请求ID</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
