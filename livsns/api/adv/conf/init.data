(1, 'hg_popBox.js', '\r\n\r\nvar hg_popBox = function(obj){\r\n	var id = obj.id;\r\n	var width = obj.para.width;\r\n	var height = obj.para.height;\r\n	var _id = document.getElementById(id);\r\n	_id.style.width = width + "px";\r\n	_id.style.height = height + "px";\r\n	_id.style.position = "absolute";\r\n	_id.style.zIndex = "9999999";\r\n	_id.style.top = "200px";//document.body.scrollTop || document.documentElement.scrollTop + document.documentElement.clientHeight/2 - height/2 +"px";\r\n	_id.style.left = document.body.offsetWidth/2 -width/2 +"px";\r\n	_id.getElementsByTagName("img")[0].style.width = width + "px";\r\n	var close = document.createElement("span");\r\n	close.style.position = "absolute";\r\n	close.style.width = 15 +"px";\r\n	close.style.height = 15 +"px";\r\n	close.style.top = -3 +"px";\r\n	close.style.right = -3 +"px";\r\n	close.style.color = "#fff";\r\n	//close.style.fontSize = "10px";\r\n	close.style.textAlign = "center";\r\n	close.style.lineHeight = "15px";\r\n	close.style.cursor = "pointer";\r\n	close.style.background = "#AC0116";\r\n	close.innerHTML = "x";\r\n	_id.appendChild(close);\r\n	close.onclick = function(){\r\n		_id.parentNode.removeChild(_id);\r\n	}\r\n	\r\n}\r\n'),
(2, 'hg_sideFloat.js', 'var hg_sideFloat = function (obj) {\r\n    var id = obj.box;\r\n    var width = obj.para.width;\r\n    var height = obj.para.height;\r\n    var top = obj.para.top ? parseInt(obj.para.top) : 0;\r\n    var _id = document.getElementById(id);\r\n    var ie = !window.XMLHttpRequest;\r\n    _id.style.position = ie ? "absolute" : "fixed";\r\n    _id.style.zIndex = "99999999";\r\n    _id.style.width = width + "px";\r\n    _id.style.height = height + "px";\r\n    if (getChildLength(_id) > 1) {\r\n\r\n        var div2 = document.createElement("div");\r\n        document.body.appendChild(div2);\r\n\r\n        if(ie){\r\n            var position = function(){\r\n                var _top = getScrollTop() + top;\r\n                if(_id){\r\n                    _id.style.top = _top + ''px'';\r\n                    _id.style.right = 0;\r\n                }\r\n\r\n                if(div2){\r\n                    div2.style.top = _top + ''px'';\r\n                    div2.style.left = 0;\r\n                }\r\n            };\r\n            window.attachEvent(''onscroll'', position);\r\n            position();\r\n        }else{\r\n            _id.style.right = 0;\r\n            _id.style.top = top + ''px'';\r\n\r\n            div2.style.left = 0;\r\n            div2.style.top = top + ''px'';\r\n        }\r\n        \r\n        var firstc = _id.children[0];\r\n        if (firstc.tagName == ''A'') {\r\n            firstc.removeChild(firstc.children[0]);\r\n            var adhtml = firstc.cloneNode(firstc);\r\n        }\r\n        else\r\n        {\r\n            var adhtml = firstc;\r\n        }\r\n\r\n        var closeC = document.createElement("span");\r\n        closeC.style.position = "absolute";\r\n        closeC.style.width = 15 + "px";\r\n        closeC.style.height = 15 + "px";\r\n        closeC.style.top = 0;\r\n        closeC.style.right = 0;\r\n        closeC.style.color = "#fff";\r\n        closeC.style.textAlign = "center";\r\n        closeC.style.lineHeight = "13px";\r\n        closeC.style.cursor = "pointer";\r\n        closeC.style.background = "#AC0116";\r\n        closeC.innerHTML = "x";\r\n        _id.appendChild(closeC);\r\n        closeC.onclick = function () {\r\n            _id.parentNode.removeChild(_id);\r\n            _id = null;\r\n        };\r\n\r\n\r\n\r\n        div2.appendChild(adhtml);\r\n        div2.style.position = ie ? "absolute" : "fixed";\r\n\r\n        div2.style.zIndex = "99999999";\r\n        div2.style.width = width + "px";\r\n        div2.style.height = height + "px";\r\n        var closeA = document.createElement("span");\r\n        closeA.style.position = "absolute";\r\n        closeA.style.width = 15 + "px";\r\n        closeA.style.height = 15 + "px";\r\n        closeA.style.top = 0;\r\n        closeA.style.left = 0;\r\n        closeA.style.color = "#fff";\r\n        closeA.style.textAlign = "center";\r\n        closeA.style.lineHeight = "13px";\r\n        closeA.style.cursor = "pointer";\r\n        closeA.style.background = "#AC0116";\r\n        closeA.innerHTML = "x";\r\n        div2.appendChild(closeA);\r\n        closeA.onclick = function () {\r\n            div2.parentNode.removeChild(div2);\r\n            div2 = null;\r\n        };\r\n    } else {\r\n        if(ie){\r\n            var position = function(){\r\n                _id.style.top = getScrollTop() + getWindowHeight() - height - 2 + ''px'';\r\n                _id.style.right = 0;\r\n            };\r\n            window.attachEvent(''onscroll'', position);\r\n            position();\r\n        }else{\r\n            _id.style.right = 0;\r\n            _id.style.bottom = 0;\r\n        }\r\n        var closeC = document.createElement("span");\r\n        closeC.style.position = "absolute";\r\n        closeC.style.width = 15 + "px";\r\n        closeC.style.height = 15 + "px";\r\n        closeC.style.top = 0;\r\n        closeC.style.right = 0;\r\n        closeC.style.color = "#fff";\r\n        closeC.style.textAlign = "center";\r\n        closeC.style.lineHeight = "13px";\r\n        closeC.style.cursor = "pointer";\r\n        closeC.style.background = "#AC0116";\r\n        closeC.innerHTML = "x";\r\n        _id.appendChild(closeC);\r\n        closeC.onclick = function () {\r\n            _id.parentNode.removeChild(_id);\r\n        };\r\n\r\n\r\n    }\r\n\r\n    function getScrollTop(){\r\n        return Math.max(document.documentElement.scrollTop, document.body.scrollTop);\r\n    }\r\n\r\n    function getWindowHeight(){\r\n        return document.documentElement.clientHeight;\r\n    }\r\n    function getChildLength(el) {\r\n        var anchor, child;\r\n        child = el.children[0];\r\n        if (child.tagName == ''A'') {\r\n            return child.children.length\r\n        } else {\r\n            return el.children.length\r\n        }\r\n    }\r\n}'),
(3, 'aa.js', 'function hg_sideFloat(obj){	var id = obj.box;	var width = obj.para.width;	var height = obj.para.height;	var top = obj.para.top ? obj.para.top : '''';	var _id = document.getElementById(id);	var ie = !window.XMLHttpRequest;		_id.style.position = ie ? "absolute" : "fixed";	_id.style.zIndex = "99999999";	_id.style.width = width + "px";	_id.style.height = height + "px";			if( _id.getElementsByTagName("img").length > 1){		//'),
(4, 'hg_adBase.js', 'function hg_AD_AddHtml(obj){var hg_addHtml=function(id,html){if(document.getElementById(id)){document.getElementById(id).innerHTML=html}else{var tem_div=document.createElement("div");tem_div.id=id;document.body.appendChild(tem_div);document.getElementById(id).innerHTML=html}};if(obj.isjs){document.write(obj.html);return}hg_addHtml(obj.box,obj.html);if(!obj.loadjs){return};var hg_loadAD_js={loadJS:function(js,_callback){var ie=document.all;var adJS=document.createElement("script");adJS.type="text/javascript";adJS.src=js;document.getElementsByTagName("head")[0].appendChild(adJS);if(ie){adJS.onreadystatechange=function(){if(adJS.readyState==''loaded''||adJS.readyState==''complete''){callback()}}}else{adJS.onload=function(){callback()}};function callback(){var ad_obj=obj;obj.loadjs=obj.loadjs.substring(0,obj.loadjs.length-3);eval(obj.loadjs+"(ad_obj)");if(typeof _callback==''function''){_callback()}}}};hg_loadAD_js.loadJS(obj.loadurl+obj.loadjs)};'),
(5, 'hg_slideUp.js', 'var hg_slideUp = function(obj){\r\n	var id = obj.box;\r\n	var time = obj.para.time/1000;\r\n	var width = obj.para.width;\r\n	var height = obj.para.height;\r\n	var top = obj.para.top;\r\n	var _id = document.getElementById(id);\r\n	_id.style.position = "absolute";\r\n	_id.style.top = top + "px";\r\n	_id.style.zIndex = "9999999";\r\n	_id.style.left = document.body.offsetWidth/2 -width/2 +"px";\r\n	_id.style.width = width + "px";\r\n	_id.style.height = height + "px";\r\n	_id.style.overflow = "hidden";\r\n	var close = document.createElement("span");\r\n	if(document.all){\r\n		close.style.styleFloat= "right";\r\n	}else{\r\n		close.style.cssFloat= "right";\r\n	}\r\n	close.style.padding = "7px";\r\n	close.style.color = "#DD364A";\r\n	close.style.fontSize = "12px";\r\n	close.innerHTML = "离广告关闭还有" + time+ "秒";\r\n	_id.insertBefore(close, _id.firstChild);\r\n	var move = setInterval(function(){\r\n		time--;\r\n		close.innerHTML = "离广告关闭还有" + time+ "秒";\r\n		if(time==0){\r\n			clearInterval(move);\r\n			slideUp(_id);\r\n		}\r\n	},1000);\r\n}\r\n\r\nvar slideUp = function(elem){\r\n	var h = elem.offsetHeight;\r\n	var move = setInterval(function(){\r\n		h = h -15;\r\n		elem.style.height = parseInt(h) +"px";\r\n		if(h < 15){\r\n			h = 0;\r\n			clearInterval(move);\r\n			elem.parentNode.removeChild(elem);\r\n		}\r\n	},16);\r\n}'),
(6, 'hg_leftFloat.js', 'function hg_leftFloat(obj){		var id = obj.box;	var width = obj.para.width;	var height = obj.para.height;	var top = obj.para.top ? obj.para.top : '''';	var _id = document.getElementById(id);	var ie = !window.XMLHttpRequest;	if(ie){		var position = function(){			var _top = getScrollTop() + parseInt(top);			if(_id){				_id.style.top = _top + ''px'';			}		};		window.attachEvent(''onscroll'', position);		position();	}	else	{		_id.style.top = top  + "px";	}	_id.style.position = ie ? "absolute" : "fixed";	_id.style.zIndex = "99999999";	_id.style.width = width + "px";	_id.style.height = height +  "px";	_id.style.left = 0;	var closeC = document.createElement("span");	closeC.style.position = "absolute";	closeC.style.width = 15 + "px";	closeC.style.height = 15 +  "px";	closeC.style.top = 0;	closeC.style.right = 0;	closeC.style.color = "#fff";	closeC.style.textAlign = "center";	closeC.style.lineHeight = "13px";	closeC.style.fontFamily = "arial";	closeC.style.cursor = "pointer";	closeC.style.background = "#AC0116";	closeC.innerHTML = "x";	_id.appendChild(closeC);	closeC.onclick = function(){							_id.parentNode.removeChild(_id);					};	function getScrollTop(){        return Math.max(document.documentElement.scrollTop, document.body.scrollTop);    }}'),
(7, 'hg_rightFloat.js', 'function hg_rightFloat(obj){\n	var id = obj.box;\n	var width = obj.para.width;\n	var height = obj.para.height;\n	var top = obj.para.top ? obj.para.top : '''';\n	var _id = document.getElementById(id);\n	var ie = !window.XMLHttpRequest;\n	\n	_id.style.position = ie ? "absolute" : "fixed";\n	_id.style.zIndex = "99999999";\n	_id.style.width = width + "px";\n	_id.style.height = height + "px";\n	\n		_id.style.right = 0;\n		_id.style.top = top + "px";\n		\n		var closeC = document.createElement("span");\n		closeC.style.position = "absolute";\n		closeC.style.width = 15 +"px";\n		closeC.style.height = 15 +"px";\n		closeC.style.top = 0;\n		closeC.style.right = 0;\n		closeC.style.lineHeight = "13px";\n		closeC.style.color = "#fff";\n		closeC.style.textAlign = "center";\n		closeC.style.fontFamily = "arial";\n		closeC.style.cursor = "pointer";\n		closeC.style.background = "#AC0116";\n		closeC.innerHTML = "x";\n		_id.appendChild(closeC);\n		closeC.onclick = function(){\n			_id.parentNode.removeChild(_id);\n		};\n	\n}\n'),
(8, 'hg_topFloat.js', '\nfunction hg_float(obj){\n	var id = obj.box;\n	console.log(obj)\n	var width = obj.para.width;\n	var height = obj.para.height;\n	var top = obj.para.top ? obj.para.top : '''';\n	var _id = document.getElementById(id);\n	_id.style.position = "absolute";\n	_id.style.zIndex = "999999999";\n	var x = obj.para.x, y =  obj.para.y;\n	var xin = true, yin = true ;\n	var step = 1 ;\n	var delay = 10 ;\n	function float_img() { \n		var L=T=0;\n		var R= document.body.clientWidth-_id.offsetWidth ;\n		var B = document.body.clientHeight-_id.offsetHeight ;\n		_id.style.left = x + document.body.scrollLeft + "px";\n        _id.style.top = y + document.body.scrollTop + "px";\n		x = x + step*(xin?1:-1) ;\n		if (x < L) { xin = true; x = L} ;\n		if (x > R){ xin = false; x = R} ;\n		y = y + step*(yin?1:-1) ;\n		if (y < T) { yin = true; y = T } ;\n		if (y > B) { yin = false; y = B };\n	} ;\n	var itl= setInterval(float_img, delay) ;\n	_id.onmouseover=function(){clearInterval(itl)} ;\n	_id.onmouseout=function(){itl=setInterval(float_img, delay)} ;\n}\n'),
(9, 'hg_float.js', 'function hg_float(obj){	var id = obj.box;	var width = obj.para.width;	var height = obj.para.height;	var top = obj.para.top ? obj.para.top : '''';	var _id = document.getElementById(id);	_id.style.position = "absolute";	_id.style.zIndex = "999999999";	var x = parseInt(obj.para.x), y =  parseInt(obj.para.y);var xin = true, yin = true ;	var step = 1 ;	var delay = 10 ;	function float_img() { 		var L=T=0;		var R= document.body.clientWidth-_id.offsetWidth ;		var B = document.body.clientHeight-_id.offsetHeight ;		_id.style.left = x + document.body.scrollLeft + "px";        _id.style.top = y + document.body.scrollTop + "px";		x = x + step*(xin?1:-1) ;		if (x < L) { xin = true; x = L} ;		if (x > R){ xin = false; x = R} ;		y = y + step*(yin?1:-1) ;		if (y < T) { yin = true; y = T } ;		if (y > B) { yin = false; y = B };	} ;	var itl= setInterval(float_img, delay) ;	_id.onmouseover=function(){clearInterval(itl)} ;	_id.onmouseout=function(){itl=setInterval(float_img, delay)} ;}');;
