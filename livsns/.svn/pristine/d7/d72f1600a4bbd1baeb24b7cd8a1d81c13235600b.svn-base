/*nav*/
.cloud-shrink .cloud-toggle{width:64px; overflow: hidden; }
.cloud-shrink .cloud-toggle a{font-size:0; }
.cloud-shrink .cloud-menu li{margin:1px 0; }
.cloud-shrink .cloud-menu a{padding:8px 0 7px 20px; }

/*aside*/
.cloud-aside{width:320px;padding:10px 15px 15px 15px;background:#f9f9f9;}
.cloud-btn{border-bottom:1px solid #e4e4e4; padding:20px 0; }
.btn-back{width: 70px;height: 36px;float: left;margin-top: 12px;line-height: 36px;padding: 0;color: #fff;margin-right: 10px;}
.cloud-list{padding-bottom: 10px; margin-bottom: 20px; border-bottom:1px solid #eee;}
.cloud-list .cloud-item{margin:10px 0; }
.cloud-list .cloud-item.pb10{padding:3px 10px;background:#fff;border-radius:3px;border: 1px solid #eee;}
.cloud-list .cloud-item .suoyin-box{width: 100%;height: 180px;background:url(../images/suoyin-default.png) no-repeat center center #fff;border: 1px solid #eee;border-radius: 3px;cursor:pointer;line-height: 176px;text-align: center;position:relative;}
.cloud-list .cloud-item .indexpic-suoyin{position: absolute;left: -2px;top: -2px;background: url("../../images/cloud/suoyintu.png") no-repeat ;width: 16px;height: 46px;}
.cloud-list .cloud-item .indexpic-suoyin-current{background-image: url("./../images/suoyintu-current.png");}
.cloud-list .suoyin-box img{vertical-align: middle;background: #fff;max-width: 170px;max-height: 170px;}
.cloud-list label{line-height:2.2; font-size:12px; color:#a4b0bc;margin:0; }
.cloud-list input{width: 118px;line-height: 28px;color: #2b3d51;border: 1px solid transparent;border-radius: 3px;text-indent: 5px;font-size: 12px;padding: 0;margin-top: 0px;height: 28px; }
.cloud-list input:focus{outline: none;}
.video-list .video-item{color:#a4b0bc; }
.video-list label{line-height:1.8; font-size:12px;letter-spacing: 1px; }

/*section*/
/*.cloud-shrink section{margin-top: 60px;}*/
.cloud-shrink .cloud-form{border:1px solid #e0e0e0;margin-top:80px; margin-bottom:40px;}
.cloud-section{padding: 20px; background-color:#fff;position: relative;}
.cloud-section .artical-title{ font-size:21px; color:#333; margin:0; text-align:center;}
.cloud-section .artical-main{margin: 10px 0 20px 0; color: #474747;}
.cloud-section .artical-main p { margin: 15px 0 0;  font-size:14px!important; color: #2f2f2f;}
.cloud-section .artical-main img {margin: 10px 0 0 0;max-width: 100%;}
.video-list-content{ margin-top:10px; }
.video-list-title{ font-size:14px; color:#a4b0bc; text-align:center; }
.video-list-content ul{ margin:0; font-size: 12px; }
.video-list-content ul li{padding:5px 0 0px 10px;  border-left:1px solid #ccc; height: auto;}
.video-list-content ul li:first-child{ padding:0 0 0 10px; }
.video-list-content ul li::before{content: '';border: 3px solid #999;background-color: #d9d9d9;display: inline-block;width: 5px;height: 5px;border-radius: 5px;margin-left: -14px;}
.video-list-content ul li .video-list-item{margin-left: 10px;border-bottom:1px solid #eee; padding-bottom: 8px;}
.video-list-content ul li:last-child .video-list-item{ border-bottom:none; }
.video-list-content ul li span{display: block; color:#999;}
.video-list-content ul li.current::before{ border: 3px solid #12950c; background-color:#d7f6d5;}
.video-list-content ul li.current span{ color:#12950c;}

.video-title input[type="text"], .video-title textarea{border-color:transparent; resize: none;}
.video-title input[type="text"]{width: 615px;line-height:25px;color: #2c85e1;display:block;border:none; }
.video-title textarea{line-height:1.6; }
.video-title .form-group{border:1px solid #eee;padding:0px 10px;border-radius: 3px;}
.video-title .form-group-title{display: block;width: 90px;height: 40px;line-height: 40px;font-size: 20px;color: #2c85e1;}
.form-title{border-radius:0; }
.video-btn{width:200px; text-align:right; }
.btn-save{width:70px;margin-top:12px; background: none;color: #fff;border: 1px solid #fff;}
.form-title{font-size:20px; }
.form-brief{font-size:12px; color:#2b3d51;border-color: #eee!important;min-height: 59px; margin-top:5px; }
.object-box{width:500px; margin:35px auto;background: #000;border-radius: 4px;}
.btn-back:hover , .btn-save:hover{background-color:#247bd8;}
/*taginput*/
.tagsinput{border-width:0; background-color:#fff; padding:5px 5px 0px 5px;border:1px solid #eee; }
.tagsinput .tag{background-color:#eee; color:#2c3d4f; padding:7px 10px 6px 17px; }
.tagsinput .tag:hover{padding-left:17px; padding-right:17px; background-color:#eee; color:#2c3d4f; }
#tagsinput_tag:focus{background:#fff;margin-top:0px!important}
.tagsinput-add{width:32px;height:28px;background-image:url('../../images/cloud/plus.png'); background-color:#eee; background-size:16px 16px; margin-bottom:5px;}

.tagsinput-add:hover{background-color:#dfe1e0; opacity:0.8; }
.tagsinput-remove-link{background:url('../../images/cloud/pop-close.png') no-repeat center center transparent; background-size:12px 12px; }
.tagsinput-remove-link:before{content:''; }
.goback{position: absolute;width: 70px;height: 30px;font-size: 14px;color: #fff;background: #2b84e0;text-align: center;line-height: 30px;border-radius: 3px;right: 10px;top: 10px;}
.goback:hover{color:#fff;opacity:0.8;}
@media only screen and (max-width: 767px){
	.cloud-shrink .cloud-form{margin:0px;}
	.cloud-aside{width: 0;padding: 0;overflow: hidden;transition: all 0.2s ease-in;}
	.cloud-aside.show{width:100%;border-right: 1px solid #e0e0e0;padding: 10px 15px 15px 15px;position: relative;}
	
	.container:before{display:none;}
	.cloud-section{position:relative;}
	.list-aside-detail{display:block;width:25px;height:25px;background:url(../images/list.png) no-repeat 3px center transparent;background-size:15px 20px;position:absolute;top:0px;left:0px;}
	.go-artical{display:none;width:25px;height:25px;background:url(../images/list.png) no-repeat 3px center #f9f9f9;background-size:15px 20px;position:absolute;top:0px;right:0px;}
	.cloud-aside.show .go-artical{display:block;}
	.cloud-section img{max-width:100%;}
	.goback{display:none;}
}


