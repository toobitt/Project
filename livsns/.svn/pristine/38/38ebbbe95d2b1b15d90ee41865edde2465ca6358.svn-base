<div class="liv_file liv_item">
	<span class="cell_name">{cell_name}</span>
	<span class="cell_file">
		<a class="btn btn-wid130 btn-operate">上传</a>
		<input type="file" name="{cell_unique_name}"/>
		<div class="file-drop">
			<div class="m2o-flex m2o-flex-center">
				<div class="file-info m2o-flex-one">
					<label>已上传：</label>
					<span class="name"></span>
				</div>
				<div class="file-set">
					<a class="view" attr="">查看</a>
					<a class="del">删除</a>
				</div>
			</div>
		</div>
	</span>
</div>