@CHARSET "UTF-8";
html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{margin:0;padding:0;border:0;outline:0;}
body{ background:#fff;font-family:"宋体";font-size:12px;}
.dis	{ display:block}
table   { border-collapse:collapse;}
img     { border:0; }
dl,dt,dd,ul,li { list-style:none;padding:0;margin:0;}
a       { font-size:12px; text-decoration:none; color:#7B7B7B; }
a:hover,ul li a:hover { text-decoration:underline;color:#0265CD} 
.clear{clear:both;overflow:visible;}
.clear:after{clear: both; content: '.';display: block;visibility:hidden;height:0;}

/*site_head*/
.site_head_top{background: url("../img/n_top_1.gif") repeat-x scroll 0 0 transparent;height: 66px;}
.site_head_top .head_menu{margin: 0 auto; width: 960px;height: 37px;}
.site_head_top .head_menu .menu_left{}
.site_head_top .head_menu .menu_left .head_logo{background: url("../img/n_logo.gif") no-repeat scroll 0 0 transparent; display: inline-block; height: 37px; width: 89px;float: left;}
.site_head_top .head_menu .menu_left .menu_left_tab{background: url("../img/n_top_2.gif") no-repeat scroll 0 0 transparent; display: inline-block; height: 37px; width: 25px;float: left;}
.site_head_top .head_menu .menu_mid{float: left; height: 37px; line-height: 37px; width: 300px;}
.site_head_top .head_menu .menu_mid .menu_tab{color:#787878;}
.site_head_top .head_menu .menu_mid .menu_tab li.default{float: left;;font-weight:600;padding: 0 10px;}
.site_head_top .head_menu .menu_mid .menu_tab li.default a{color:#787878;}
.site_head_top .head_menu .menu_mid .menu_tab li.line-1{float: left; height: 37px; width: 1px;background: url("../img/n_top_7.gif") no-repeat scroll 0 0 transparent;}
.site_head_top .head_menu .menu_mid .menu_tab li a{ letter-spacing: 0.12em;}
.site_head_top .head_menu .menu_mid .menu_tab li a:hover{color:#029ee8; letter-spacing: 0.12em;text-decoration: none;}
.site_head_top .head_menu .menu_mid .menu_tab li.cursor {float: left;;font-weight:600;padding: 0 10px;}
.site_head_top .head_menu .menu_mid .menu_tab li.cursor a{color:#029ee8; letter-spacing: 0.12em;text-decoration: none;}
.site_head_top .head_menu .menu_mids{line-height: 37px; background: url("../img/n_top_4.gif") no-repeat scroll 0 center transparent; float: left; height: 37px; width: 218px;}
.site_head_top .head_menu .menu_mids input.sea_name{ background: none repeat scroll 0 0 transparent; border: medium none; color: #CCCCCC; height: 20px; margin-left: 1px; padding-left: 4px; width: 164px;}
.site_head_top .head_menu .menu_mids input.sea_sub{background: url("../img/n_top_3.gif") no-repeat scroll 0 center transparent; border: medium none; float: right; height: 16px; margin-right: 2px; margin-top: 11px; *margin-top: -27px;vertical-align: middle; width: 40px;}
#sea_name{display: none;left: 0; width: 215px;margin: -7px 0 0 1px;*margin: -4px 0 0 1px;}
#sea_name ul{ background: none repeat scroll 0 0 #FFFFFF; border: 1px solid #CCCCCC; margin: -4px 0 0 1px; position: relative; width: 215px; z-index: 9999;}
#sea_name ul li{color: #999999; cursor: pointer; height: 20px; line-height: 20px; padding-left: 6px;}
#sea_name ul li a{color:#999;}
.site_head_top .head_menu .menu_right{float: right; padding-top: 8px; width: 300px;}
.site_head_top .head_menu .menu_right a.login{ margin-right: 5px; background: url("../img/n_top_5.gif") no-repeat scroll 0 center transparent; display: inline-block; float: right; height: 21px; width: 42px;}
.site_head_top .head_menu .menu_right a.register{background: url("../img/n_top_6.gif") no-repeat scroll 0 center transparent; display: inline-block; float: right; height: 21px; width: 42px;}
.site_head_top .head_menu .menu_right .login_user{color: #8D95A0; line-height: 20px; padding: 0; text-align: left;}
.site_head_top .head_menu .menu_right .login_user a{color: #8D95A0;}
.site_head_top .head_menu .menu_right .login_user a:hover{color: #029ee8;}
.site_head_top .head_menu .menu_right ul{float: right; *margin-top: -20px; margin-right: 30px;}
.site_head_top .head_menu .menu_right ul li{position: relative;float: left;width: 47px;margin-left: 5px}
.site_head_top .head_menu .menu_right li ul{position: absolute;left: 150px;top: 0;display: none;margin-top: 0px;*margin-top: 17px}
.jiange_li{width: 2px;float: left}
.site_head_top .head_menu .menu_right .iwant{text-align: left;height: 21px; line-height: 21px;background: url(../img/iwant.png) no-repeat;width: 41px;padding-left: 6px;z-index: 20;}
.site_head_top .head_menu .menu_right .user_setting,.site_head_top .head_menu .menu_right .user_logout{height: 21px; line-height: 21px;background: url("../img/logoset.png") no-repeat scroll 0 0 transparent;text-align: center;width: 41px}
.site_head_top .head_menu .menu_right .iwant a,.site_head_top .head_menu .menu_right .user_setting a,.site_head_top .head_menu .menu_right .user_logout a{color: #fff}
.site_head_top .head_menu .menu_right .iwant a:hover,.site_head_top .head_menu .menu_right .user_setting a:hover,.site_head_top .head_menu .menu_right .user_logout a:hover{color: #fff;text-decoration: underline}
.site_head_top .head_menu .menu_right .iwant li a:hover{color:#A5A3A4;text-decoration:none;}
.site_head_top .head_menu .menu_right li#head_n{background:url(../images/sx_03.jpg) repeat scroll 0% 0% transparent; width: 45px; height: 21px; color: #fff; text-align: center; line-height: 21px; display: inline-block;}
.site_head_top .head_menu .menu_right li#head_n a{color:#fff;}
.site_head_top .head_menu .menu_right li:hover ul{display: block;}
.site_head_top .head_menu .menu_right li ul.lists{background: none repeat scroll 0 0 #F5F5F5; border: 1px solid #D2D2D2; color: #A5A3A4; height: 55px; left: -75px; padding: 5px 5px 10px; position: absolute; top: auto; width: 110px; z-index: 100;}
.site_head_top .head_menu .menu_right li ul.lists li{ background: url("../img/xuxian_03.jpg") no-repeat scroll 0 18px transparent; float: left; height: 21px; line-height: 21px; margin: 0; text-align: left; width: 52px;}
.site_head_top .head_menu .menu_right li ul.lists li a{color: #A5A3A4; display: inline-block; margin-right: 0; text-decoration: none; width: 40px;}
.site_head_top .head_menu .menu_right li ul.lists li span{ color: #A5A3A4; float: right; padding-right: 3px; width: 1px;}
.site_head_top .head_menu .menu_right li ul.lists li.line-2{background: none repeat scroll 0 0 transparent; color: #A5A3A4; display: inline-block; float: left; margin: 0 4px 0 0; width: 1px;}
.site_head_top .tab_menu{margin: 0 auto; width: 960px;height: 29px;}

/* my site*/
.menus{ height: 27px; margin: 0 auto; padding: 6px 0; width: 960px;line-height: 27px;}
.menus ul.menu_ul{}
.menus ul.menu_ul li.prefix{background: url("../img/n_mid_1.gif") no-repeat scroll 0 0 transparent; height: 27px; width: 4px; float: left;}
.menus ul.menu_ul li.default{padding: 0 5px; color:#5b5b5b; background: url("../img/n_mid_2.gif") repeat-x scroll 0 0 transparent; height: 27px; float: left;}
.menus ul.menu_ul li.default a{color:#008bde;}
.menus ul.menu_ul li.interval{background: url("../img/n_mid_4.gif") no-repeat scroll 0 0 transparent; height: 27px; width: 14px; float: left;}
.menus ul.menu_ul li.suffix{ background: url("../img/n_mid_3.gif") no-repeat scroll 0 0 transparent; float: left; height: 27px; width: 229px;}

.sublink{color: #939393;height: 30px; margin: 0 auto; width: 980px;}
.sublink li{display: inline;line-height: 30px;text-align: left}
.sublink li a{color: #939393;margin: 0 10px 0 8px}
.sublink li.cur a{color: #007ED6}