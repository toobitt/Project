.lightbox{width:392px;height:auto;display:none;margin:0 auto;width:700px;padding:10px}
.lightbox h3{background:#E5E5E5;padding:0px 15px 5px 15px;height:23px;line-height:23px;font-size:12px;font-weight:bold}
.lightbox .text{padding: 10px 25px 20px 45px;margin:0;background:#fff;color:#7e7e7e;height:250px;width:614px;}
.bs{border:1px solid #D4D4D4;width:495px;height: 24px;line-height: 24px;}
.lightbox .text ul{*height:145px;height:120px;}
.lightbox .text ul li{margin-bottom:7px;color:#000;}
.lightbox .text ul li span.cs{display: inline-block; float: left; margin-top: 5px;}
.lightbox .text ul li.program_bt{text-align:center;}
.lightbox .text ul li.program_bt input{background: url("../img/hg_bg_14.gif") no-repeat scroll 0 0 transparent; border: medium none; color: #FFFFFF; font-weight: 600; height: 27px; margin-right: 10px; width: 60px;}
.lightbox .text .video_list{width: 590px;float:left;}
.lightbox .text .video_list h4 {padding-left: 50px;}
.lightbox .text .video_list .video-list{width:594px; height:auto;}
.lightbox .text .video_list .video-list li{width:173px;float:left;color:#0265ca;padding: 8px 0 0 5px;}
.lightbox .text .video_list .video-list li a{color:#0265ca;}
.lightbox .text .video_list .video-list li img{padding-left:4px;}
#p_brief{*width:525px;}
.lightbox .text .video_list .video_title{width:594px;height: 31px;background:url("../img/program_add_bg.jpg") no-repeat scroll 0 0 transparent;font-size:14px;float:left;}
.lightbox .text .video_list .video_title li{ width: 71px;margin-top: 3px;float:left;padding-top: 7px;text-align:center;line-height:18px;}
.lightbox .text .video_list .video_title li.video_title_now{margin-left: 10px;margin-top: 3px;width: 81px;height: 28px;float:left;padding-top: 7px;background:url("../img/program_add_now.jpg") no-repeat scroll 0 0 transparent;}
.lightbox .text .video_list .video_title li.video_title_now a{font-weight: 600;}

.lightbox_top{background:url(../img/Rounded.png) 0 -266px no-repeat;height:16px;font-size:0}
.lightbox_middle{padding:0 8px;background:url(../img/zf_bg.png) repeat-y;width:auto}
.lightbox_bottom{background:url(../img/Rounded.png) 0 -283px no-repeat;height:16px;font-size:0}
.box{width:490px;height:auto;background:#FFFFFF;border:5px solid #ccc;display:none; margin:0;}
.box dt{background:#f4f4f4;padding:5px;}
.box dd{padding:20px; margin:0;}
.box input{width:100px;height:30px;font-size:16px;margin-left:340px;}

.lightbox .text .video_list .pagelink{width:auto;margin:5px 0 0 0;float:right;}
.lightbox .text .video_list .pagelink .pages{height:auto;}
.lightbox .text .video_list .pagelink .pages li{margin:0px;padding:0px;float:left;clear:none;background:none;border:0px;}
.lightbox .text .video_list .pagelink .pages li a{ background: none repeat scroll 0 0 #FFFFFF;border: 1px solid #9E9E9E;clear: none;float: left;line-height: 18px;margin-left: 5px;padding:0 5px;width: auto;}
.lightbox .text .video_list .pagelink .pages li a.pages_current{background: none repeat scroll 0 0 #dadada;border: 1px solid #9E9E9E;clear: none;float: left;line-height: 18px;margin-left: 5px;padding: 0 5px;width: auto;}
.lightbox .text .video_list .pagelink .pages li a:hover{background: none repeat scroll 0 0 #FFFFFF;border: 1px solid #0065ca;clear: none;float: left;line-height: 18px; margin-left: 5px;padding: 0 5px;width: auto;}
.lightbox .text .video_list .page_total a{-moz-border-radius-topleft:5px;-moz-border-radius-bottomleft:5px;-webkit-border-top-left-radius:5px;-webkit-border-bottom-left-radius:5px;border-top-left-radius:5px;border-bottom-left-radius:5px;}
.lightbox .text .video_list .p_l a{-moz-border-radius-topright:5px;-moz-border-radius-bottomright:5px;-webkit-border-top-right-radius:5px;-webkit-border-bottom-right-radius:5px;border-top-right-radius:5px;border-bottom-right-radius:5px;}
.lightbox .text .video_list .pagelink .pages li .pages_current{padding:0 10px 0 10px;line-height:30px;width:auto;float:left;clear:none;background:none repeat scroll 0 0 #EFF9FB;border:0px;text-decoration:none;}

