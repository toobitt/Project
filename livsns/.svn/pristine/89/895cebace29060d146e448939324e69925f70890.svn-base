html{width:100%;height:100%;}body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,select,p,blockquote,th,td{margin:0;padding:0}table{border-collapse:collapse;border-spacing:0}fieldset,img{border:0}address,button,caption,cite,code,dfn,em,input,optgroup,option,select,strong,textarea,th,var{font:inherit}del,ins{text-decoration:none}li{list-style:none}caption,th{text-align:left}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal}q:before,q:after{content:''}abbr,acronym{border:0;font-variant:normal}sup{vertical-align:baseline}sub{vertical-align:baseline}legend{color:#000}
a{text-decoration:none;color:#333;}
.overflow{white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
input[type="text"]:focus{outline:none;box-shadow:none;}
input[type="text"]{outline: none;box-shadow: none;-webkit-appearance:none;}
.w8{width:8%;}
.w9{width:9%;}
.w10{width:10%;}
.w11{width:11%;}
.w12{width:12%;}
.w15{width:15%;}
.w18{width:18%;}
.w20{width:20%;}
.w25{width:25%;}
.w30{width:30%;}

/* 盒模型 */
.sys-flex{display:-webkit-box;width:auto;display:-ms-flexbox;}
.sys-flex-center{-webkit-box-align:center;-ms-flex-align: center;}
.sys-flex-end{-webkit-box-align:end;-ms-box-align:end;box-align:end;}
.sys-flex-start{-webkit-box-align:start;-ms-box-align:end;box-align:start;}
.sys-flex-one{-webkit-box-flex:1;-ms-flex:1;}
.sys-flex-two{-webkit-box-flex:2;-ms-flex:2;}
.sys-vertical{-webkit-box-orient:vertical;-ms-flex-direction:column;}
.sys-flex-overflow{flex-wrap:wrap;}

.overhidden{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
body{width:260px;height:100%;margin:0 auto;background:url(./bg.png) no-repeat center center;background-size:100% 150%;font-family: 思源, 'Helvetica Neue', Helvetica, Arial, sans-serif;}
.lottery-container{height:100%;overflow:hidden;}

.content-box .current-seller-num{width:100%;height: 35px;line-height: 35px;color: #896d62;margin: 20px 0px 5px 0px;}
.content-box .current-seller-num a{display:block;width:120px;color: #896d62;}
.content-box .current-seller-num .logout{display:block;width: 45px;height: 100%;text-align: right;color:#dd3c3f;font-weight:bold;}
.award-box{width:100%;height:216px;margin:0 auto;background:url(./award.png) no-repeat center top;background-size:cover;position:relative;}
.award-box .award-pic{width:60%;height:60px;margin: 12% 20%;}
.award-box span{display:block;width:100%;text-align:center;}
.award-box span.award-tip{color:#f4df4f;margin-top: 10px;font-size:14px;}
.award-box span.award{color:#eebd4f;margin-top: 5px;font-size:18px;font-weight:bold;}
.award-info .award-info-tip{display: block;padding:15px 0px 5px 0px;margin-bottom: 15px;color: #896d62;font-size: 18px;border-bottom: 1px solid #e4c7a6;}
.award-info .award-item{color:#8b6d62;font-size:18px;margin:5px 0px;}
.award-info .btn{display: block;width: 100%;height: 40px;margin-top: 20px;color: #fff;text-align: center;line-height: 40px;font-size: 18px;border-radius: 2px;}
.award-info .btn.has-confirm{background:#bbb;}
.award-info .btn.confirm-btn{background: #f5c644;border-bottom: 2px solid #ca9f3a;}
.award-info .check-history{display: block;text-align: center;font-size: 14px;color: #a0877d;height: 40px;line-height: 40px;text-decoration: underline;}

.record-box{width: 100%;height: 100%;position: fixed;top: 100%;left: 0px;z-index: 999;  transition: all 0.2s ease-in;background:url(./bg.png) no-repeat center center;background-size:100%;}
.record-box .record-nav{padding: 0px 15px 0px 15px;position: relative;}
.record-box .record-nav span{display:block; margin-top: 20px;text-align: center;color: #896d62;font-size: 20px;padding-bottom:5px;border-bottom: 1px solid #e4c7a6;}
.record-box .record-nav a{display:block;width:25px;height:25px;background:url(./icon_close-2x.png) no-repeat center center;background-size:40px;  position: absolute;right: 10px;bottom: 20px;}
.record-box .record-list{padding:0px 25px;  color: #896d62;font-size: 16px;}
.record-box .record-list li{width:100%;border-bottom:1px solid #e4c7a6;padding:5px 0px;}
.record-box .record-list img{width:80px;height:80px;border-radius:3px;vertical-align: middle;}
.record-box .record-list-info{padding-left:10px;}
.record-box .record-list-info span{display: block;height: 25px;line-height: 25px;padding: 0px 10px 0px 0px;}
.record-box .no-record{display: block;text-align: center;padding: 20px 0px;font-size: 18px;}

.content-box{display:none;}
.seller-box{width: 100%;height: 100%;background:url(./bg.png) no-repeat center center;background-size:100%;position: fixed;z-index: 999;left: 0;top: 0px;display:none;transition: all 0.2s ease-in;}
 
.seller-box .lottery-title{display:block;width:100%;height:30px;line-height:30px;text-align:center;font-size:24px;font-weight:bold;color: #896d62;margin-top:30px;}
.seller-box .seller-box-item{height:150px;padding:20px; margin-top: calc(50% - 75px);}
.seller-box .seller-box-tip{display: block;color: #896d62;font-size: 18px;margin: 0px 0px 20px 0px;}
.seller-box .seller-box-item input[type="text"]{display:block;width:100%;height:40px;font-size:14px;text-indent:10px;border:1px solid #e4c7a6;border-radius:3px;}
.seller-box .seller-box-item .login{display:block;width:100px;height:40px;  margin: 20px auto;line-height:40px;border-radius:3px;background: #f5c644;color:#fff;font-size:18px;border-bottom: 2px solid #ca9f3a;text-align:center;}
