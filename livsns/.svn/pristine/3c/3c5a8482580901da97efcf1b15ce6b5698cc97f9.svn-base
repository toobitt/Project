<?phprequire('./global.php');class activity_node_type extends appCommonFrm{	public function __construct()	{		parent::__construct();	}	public function __destruct()	{		parent::__destruct();	}		//获取圈子 节点	public function show()	{		$this->setXmlNode('activity_types' , 'activity_type');		foreach($this->settings['activity_type'] as $k=>$v)		{			$result = array(				'id' => $k,				"name" => $v,				"fid" => 0,				"depth" => 1,				'attr' => $this->settings['activity_type_attr'][$k],				'input_k' => '_type',				'is_last' => 1			);			$this->addItem($result);		}		$this->output();	}		}/** * 程序入口 */$out = new activity_node_type();$action = $_INPUT['a'];if(!method_exists($out, $action)){	$action = 'show';	}$out->$action();?>