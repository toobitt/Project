@charset "utf-8";
/* CSS Document */
/*鍏ㄥ眬鏍峰紡*/
html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{margin:0;padding:0;border:0;outline:0;}
h1{width:284px;float:left;margin:35px 0 0 25px;}
body{ background:#fff;font-family:"宋体";font-size:12px;}
.dis	{ display:block}
table   { border-collapse:collapse;}
img     { border:0; }
dl,dt,dd,ul,li { list-style:none;padding:0;margin:0;}
a       { font-size:12px; text-decoration:none; color:#7B7B7B; }
a:hover,ul li a:hover { text-decoration:underline;color:#0265CD} 
.clear{clear:both;overflow:visible;}
.clearit{clear:both;margin:0px;padding:0px;}
.clear:after{clear: both; content: '.';display: block;visibility:hidden;height:0;}
.clear2{clear:both;font-size:0;line-height:0;height:0;overflow:hidden;}
input{padding:3px 4px;outline:none;-moz-outline-style: none;}
input::-moz-focus-inner{outline:none}
input:focus{outline:none;}
a{blr:expression(this.onFocus=this.blur());/* IE Opera */outline:none;   /*    FF Opera    */}
a:focus{-moz-outline-style: none;   /*  FF  */}
.hidden{display:none;}
.show{}
.overflow{overflow:hidden; 
white-space:nowrap; 
text-overflow:clip; /* for IE */ 
-o-text-overflow: clip; /* for Opera */ 
-icab-text-overflow: clip; /* for iCab */ 
-khtml-text-overflow: clip; /* for Konqueror Safari */ 
-moz-text-overflow: clip; /* for Firefox,mozilla */ 
-webkit-text-overflow: clip; /* for Safari,Swift*/ }

