package com.hogesoft.wowza.exceptions;

import com.hogesoft.wowza.utils.ExceptionMessage;

public final class UndefinedActionTypeException extends MediaException {

	private static final long serialVersionUID = 1L;

	public UndefinedActionTypeException(String message) {
		super(ExceptionMessage.UndefinedActionType + ": " + message);
	}

	public UndefinedActionTypeException() {
		super(ExceptionMessage.UndefinedActionType);
	}
}
