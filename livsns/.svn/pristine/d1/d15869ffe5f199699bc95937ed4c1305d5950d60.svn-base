<div class="liv_choose liv_item">
	<span class="cell_choose">
		<span class="cell_element {cell_options_other_sign} {cell_options_noplay}">
			<input type="radio" name="{cell_unique_name}" value="{cell_options_id}" id="cell_choose{cell_id}{cell_key}"/><label for="cell_choose{cell_id}{cell_key}">{cell_options_name}</label>
			<div class="brief">{cell_options_describes}</div>
			{cell_options_other}
		</span>
	</span>
	<span class="cell_input">
		<input type="text" name="{cell_unique_name}" placeholder="{cell_brief}" value="" id="cell_choose{cell_id}{cell_key}"/>
	</span>
</div>