.py-submit, .py-huifu, .py-change, .py-watch, .py-format{line-height:24px;height:24px;padding:0 8px;margin:10px 0 0 10px;display:inline-block;cursor:pointer;border-radius:3px;font-size:12px;color:#fff;}
