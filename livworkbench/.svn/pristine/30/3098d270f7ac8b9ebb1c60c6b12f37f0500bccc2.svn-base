.py-dbox{background:#474747;text-align:center;padding:30px 0;display:none;}
.py-d-option{margin-top:15px;}
.py-d-ok, .py-d-no{cursor:pointer;text-align:left;position:relative;display:inline-block;text-indent:45px;width:90px;height:35px;line-height:35px;border:1px solid #424242;border-radius:5px;}
.py-d-no{margin-left:10px;}
.py-d-ok:before, .py-d-no:before{
    content:'';
    position:absolute;width:18px;height:18px;left:18px;top:50%;border-radius:9px;margin-top:-9px;background:url('../image/ok.png') no-repeat center;
}
.py-d-no:before{
    background-image:url('../image/cancel.png');
}
.py-d-ok{
    background-image: -webkit-repeating-linear-gradient(#df6059, #d85959 50%, #d0534d);
    background-image: -moz-repeating-linear-gradient(#df6059, #d85959 50%, #d0534d);
    background-image: repeating-linear-gradient(#df6059, #d85959 50%, #d0534d);
}
.py-d-ok:hover{
    background-image: -webkit-repeating-linear-gradient(#d0534d, #cd534d 50%, #df6059);
    background-image: -moz-repeating-linear-gradient(#d0534d, #cd534d 50%, #df6059);
    background-image: repeating-linear-gradient(#d0534d, #cd534d 50%, #df6059);
}
.py-d-no{
    background-image: -webkit-repeating-linear-gradient(#616161, #5c5a5b 50%, #555);
    background-image: -moz-repeating-linear-gradient(#616161, #5c5a5b 50%, #555);
    background-image: repeating-linear-gradient(#616161, #5c5a5b 50%, #555);
}
.py-d-no:hover{
    background-image: -webkit-repeating-linear-gradient(#5c5a5b, #555 50%, #616161);
    background-image: -moz-repeating-linear-gradient(#5c5a5b, #555 50%, #616161);
    background-image: repeating-linear-gradient(#5c5a5b, #555 50%, #616161);
}

