html{color:#000;}body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,select,p,blockquote,th,td{margin:0;padding:0}table{border-collapse:collapse;border-spacing:0}fieldset,img{border:0}address,button,caption,cite,code,dfn,em,input,optgroup,option,select,strong,textarea,th,var{font:inherit}del,ins{text-decoration:none}li{list-style:none}caption,th{text-align:left}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal}q:before,q:after{content:''}abbr,acronym{border:0;font-variant:normal}sup{vertical-align:baseline}sub{vertical-align:baseline}legend{color:#000}

html{min-height:770px;}
.hide{display:none!important;}
.navbar{height:64px;background-color:#399ce1;color:#fff;}
.navbar-fixed .page-content{padding-top:64px;overflow:hidden;}
.navbar .center{font-size:1.6em;font-weight:normal;}
.main-frame{width:100%;height:100%;}
.panel{width:390px;}
body.with-panel-left-reveal .panel-overlay{margin-left:390px;}
body.with-panel-left-reveal .views{-webkit-transform:translate3d(390px,0,0);-ms-transform:translate3d(390px,0,0);transform:translate3d(390px,0,0);}

/*侧栏*/
i.icon.icon-bars-white{background-image:url(../images/navbar_icon_modules_white_2x.png);background-size:24px 18px;width:24px;height:18px;}
/*回退*/
i.icon.icon-back-white{background-image:url(../images/navbar_icon_back_2x.png);background-size:44px 44px;width:44px;height:44px;}
/*close*/
i.icon.icon-close-white{background-image:url(../images/navbar_icon_cancel.png);background-size:44px 44px;width:44px;height:44px;}

/*加载等待*/
.preloader-indicator-modal{margin-left:-59px;margin-top:-59px;}
.preloader-indicator-modal .preloader{width:54px;height:54px;background-image:url(../images/loading.gif);background-size:54px 54px;}

/*侧栏样式*/
.panel-left .content-head{position:relative;background-color:#399ce1;color:#fff;height:64px;padding:0 10px;line-height:64px;background-image:url(../images/loading_default_bg.png);background-size:118px 40px;background-position:left center;background-repeat:no-repeat;font-size:0;}
.panel-left .head-shadow{position:absolute;height:100%;width:10px;top:0;right:0;background-image:url(../images/homepage_shadow_right.png);background-size:10px 284px;}
.panel-left .content-block{margin:0;padding:0;font-size:0;}
.panel-left .row{border-bottom:1px solid #666;}
.panel-left .panel-left-menu{width:130px;height:130px;background-size:130px 130px;border-right:1px solid #666;margin-left:0!important;}
.panel-left .panel-left-menu a{display:block;width:100%;height:100%;}
.panel-left .menu-news{background-image:url(../images/m_xinwen.png);}
.panel-left .menu-live{background-image:url(../images/m_zhibo.png);}
.panel-left .menu-zhengwu{background-image:url(../images/m_zhengwu.png);}
.panel-left .menu-road{background-image:url(../images/m_lukuang.png);}
.panel-left .menu-picture{background-image:url(../images/m_tupian.png);}
.panel-left .menu-bike{background-image:url(../images/m_bike.png);}
