<script>
$(function(){
	/*vedio switch*/
	$('.tab-nav').switchable({
		            triggers:false,
				    effect: 'scrollLeft',
				    steps: 1,
				    panels: 'li',
				    easing: 'ease-in-out',
				    visible: 1, // important
				    loop: true,
				    end2end: true,
				    autoplay:true,
				    interval:3,
				    prev: $(this).find('.prev'),
				    next: $(this).find('.next'),
				    onSwitch: function(event, currentIndex) {
				        var api = this,
				            len=this.length;
				        api.prevBtn.toggleClass('disabled', currentIndex === 0);
				        api.nextBtn.toggleClass('disabled', currentIndex === len - 1);
				        $('.tab-nav li').eq(currentIndex).addClass('hover').siblings().removeClass('hover');
				        $('.con-list li').eq(currentIndex).addClass('show').siblings().removeClass('show');
				    }
	  });
	  /*vedio tab*/
	  $('.tab-nav ul li').each(function(index){
             $(this).on('hover',function(){
                 $(this).addClass('hover').siblings().removeClass('hover');
                 $('.con-list li').eq(index).addClass('show').siblings().removeClass('show');
             });
	  });
	 /*rank tab*/
	 $('.vedio-ranking').each(function(){
          var self=$(this);
          self.on('hover','.ranking-tab a',function(){
              self.find('.ranking-tab a').removeClass('hover');
              $(this).addClass('hover');
              self.find('.ranking-con').removeClass('show');
              self.find('.ranking-con'+$(this).data('id')).addClass('show');
          });
	 });

	 /*影展*/
	 $('.movie-show .show-list').switchable({
		    triggers:false,
		    effect: 'scrollLeft',
		    steps: 1,
		    panels: 'li',
		    easing: 'ease-in-out',
		    visible: 4, // important
		    loop: true,
		    end2end: true,
		    autoplay:true,
		    interval:2,
		    prev: $('.movie-prev'),
		    next: $('.movie-next')
     });
});
</script>
