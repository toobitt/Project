#my-tool-resize-content div{margin-left:10px;color:#fff;font-size:12px;margin-bottom:15px;}
#my-tool-resize-content div:last-child{margin-bottom:0;position:relative;}
#my-tool-resize-content div:last-child:after{content:'';position:absolute;left:0;top:0;right:0;bottom:0;z-index:1;}
#my-resize-width, #my-resize-height{color:#fff;width:50px;border:1px solid #3e3e3e;background:#686868;height:18px;line-height:18px;padding:0;outline:none;}
