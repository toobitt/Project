.avpw_is_fullscreen .avpw{box-shadow:0 6px 44px rgba(0, 0, 0, 0.7);}

#avpw_history{border:1px solid #595959;width:71px;height:27px;border-radius:2px;float:none;position:relative;left:0;top:0;margin:0;}
#avpw_history:after{
    content:'';
    position:absolute;
    width:1px;
    height:27px;
    left:35px;
    top:0;
    background:#595959;
}
.avpw .avpw_history_button{border:none;border-radius:0;box-shadow:none;width:35px;height:27px;}

#avpw_history_undo{background:url(../images/my/undo-can.png) no-repeat center;}
#avpw_history_undo.avpw_history_disabled{background-image:url(../images/my/undo.png);}
#avpw_history_redo{margin-left:1px;background:url(../images/my/redo-can.png) no-repeat center;}
#avpw_history_redo.avpw_history_disabled{background-image:url(../images/my/redo.png);}
#avpw_history_undo .avpw_history_icon, #avpw_history_redo .avpw_history_icon{display:none;}
#avpw_history_undo_blocker, #avpw_history_redo_blocker{width:36px;height:29px;}
#avpw_history_undo.avpw_button_down, #avpw_history_redo.avpw_button_down{box-shadow:none;}

#my-controls{position:relative;z-index:10000;height:50px;}
#my-cancel{display:none;}
#my-cbig{height:50px;padding-left:15px;}
#my-cbig li{float:left;line-height:50px;height:100%;cursor:pointer;}
#my-cbig li.on{color:#0a58aa;font-weight:bold;position:relative;}
#my-cbig li.on:after{
    content:"";
    position:absolute;
    bottom:0;
    left:50%;
    margin-left:-10px;
    border-left:10px transparent solid;
    border-right:10px transparent solid;
    border-bottom:10px solid #1e1e1e;
    height:0;
    width:0;
}
#my-csmall{position:absolute;left:25px;top:60px;text-align:center;}
#my-csmall .my-cseach{background:#4d4d4d;border-radius:10px;width:75px;display:none;}
#my-csmall .my-cseach *{color:#fff !important;font-size:12px !important;}
.my-specal-content li{padding:12px 0;position:relative;color:#fff;font-size:12px;cursor:pointer;margin-bottom:1px;}
.my-specal-content li:first-child{border-radius:10px 10px 0 0;}
.my-specal-content li:last-child{border-radius:0 0 10px 10px;}
.my-specal-content li:after{
    content:"";
    width:70%;
    height:1px;
    background:#585858;
    position:absolute;
    left:50%;
    bottom:-1px;
    margin-left:-35%;
}
.my-specal-content li:last-child:after{display:none;}
.my-specal-content li:hover, .my-specal-content li.on{background:#686868;}
.my-specal-content li:hover:after, .my-specal-content li.on:after{display:none;}

