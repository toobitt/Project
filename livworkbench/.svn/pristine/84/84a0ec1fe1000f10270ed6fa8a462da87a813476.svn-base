#my-text-word:focus{outline:none;box-shadow:none;}
#my-text-color-table, #my-drawing-color-table{position:absolute;left:70px;top:10px;padding-left:15px;display:none;width:116px;}
#my-text-color-table ul, #my-drawing-color-table ul{background:#686868;padding:5px 5px 4px;float:left;}
#my-text-color-table li, #my-drawing-color-table li{float:left;width:18px;height:13px;line-height:1;margin:0 4px 1px 0;cursor:pointer;}
#my-text-color-table li.nomargin, #my-drawing-color-table li.nomargin{margin-right:0;}
#my-text-add{display:block;margin:10px auto 0;width:53px;height:24px;line-height:24px;background:#3e3e3e;border-radius:3px;text-align:center;cursor:pointer;}


.my-brush-box{position:absolute;padding-left:15px;width:220px;left:70px;top:18px;display:none;}
.my-brush-box ul{float:left;background:#686868;}
.my-brush-box .my-brush-each{float:left;position:relative;height:52px;padding:0 5px;}
.my-brush-bold .my-brush-each:after{
    content:'';
    position:absolute;
    left:50%;
    top:50%;
    border:1px solid #fff;
}
.my-brush-bold .my-brush-each-5{width:5px;}
.my-brush-bold .my-brush-each-5:after{
    height:5px;
    width:5px;
    border-radius:3px;
    margin:-3px 0 0 -3px;
}

.my-brush-bold .my-brush-each-15{width:10px;}
.my-brush-bold .my-brush-each-15:after{
    height:9px;
    width:9px;
    border-radius:5px;
    margin:-5px 0 0 -5px;
}

.my-brush-bold .my-brush-each-25{width:15px;}
.my-brush-bold .my-brush-each-25:after{
    height:13px;
    width:13px;
    border-radius:7px;
    margin:-7px 0 0 -7px;
}

.my-brush-bold .my-brush-each-35{width:20px;}
.my-brush-bold .my-brush-each-35:after{
    height:17px;
    width:17px;
    border-radius:9px;
    margin:-9px 0 0 -9px;
}

.my-brush-bold .my-brush-each-45{width:25px;}
.my-brush-bold .my-brush-each-45:after{
    height:21px;
    width:21px;
    border-radius:11px;
    margin:-11px 0 0 -11px;
}

.my-brush-bold .my-brush-each-60{width:30px;}
.my-brush-bold .my-brush-each-60:after{
    height:25px;
    width:25px;
    border-radius:13px;
    margin:-13px 0 0 -13px;
}

.my-brush-bold .my-brush-each:hover{background:#4d4d4d;}
#my-drawing-bold{display:inline-block;height:40px;width:40px;position:relative;}
#my-drawing-bold:hover{background:none;}
#my-tool-drawing-content .my-drawing-each{padding:10px 0;position:relative;}
#my-tool-drawing-content .my-drawing-each.each-bold{padding-bottom:0;}
#my-tool-drawing-content .my-drawing-each:hover{background:#686868;}
#my-tool-drawing-content .my-drawing-each:hover:after{display:none;}
#my-tool-drawing-content .my-drawing-each:first-child{border-radius:10px 10px 0 0;padding-top:10px;}
#my-tool-drawing-content .my-drawing-each:last-child{border-radius:0 0 10px 10px;padding-bottom:10px;}
#my-tool-drawing-content .each-color:hover #my-drawing-color-table{display:block;}
#my-tool-drawing-content .each-bold:hover #my-drawing-bold-table{display:block;}

#my-tool-drawing-content .my-drawing-each:after{
    content:'';
    position:absolute;
    left:50%;
    bottom:-1px;
    height:1px;
    width:70%;
    margin-left:-35%;
    background:#595959;
}
#my-tool-drawing-content .my-drawing-each:last-child:after{
    display:none;
}

#my-drawing-xp{vertical-align:top;display:inline-block;margin-top:5px;width:24px;height:24px;background:url(../images/my/drawxp.png) no-repeat;}

.my-more-each{position:relative;padding:4px 0;cursor:pointer;}
.my-more-each:after{
    content:'';
    width:70%;
    height:1px;
    background:#585858;
    position:absolute;
    left:50%;
    bottom:-1px;
    margin-left:-35%;
}
.my-more-each:last-child:after{
    display:none;
}
.my-more-each.on{background:#686868;}
.my-more-each.on:after{display:none;}
.my-more-each:first-child{border-radius:10px 10px 0 0;padding-top:10px;}
.my-more-each:last-child{border-radius:0 0 10px 10px;padding-bottom:10px;}
.my-more-each .my-more-icon{display:inline-block;width:21px;height:21px;margin:3px 0 1px;}
.my-more-gongju{position:absolute;left:70px;top:50%;margin-top:-5px;padding-left:15px;width:200px;opacity:0.5;}
.my-more-gongju.my-brush-box{margin-top:-26px;opacity:1;}
.my-more-gongju .avpw_slider_positioned{left:10px;right:10px;height:9px;padding:0;border-radius:3px;background:#95A0A6;}
.my-more-gongju .avpw_slider_positioned_inner{border:none;height:9px;padding:0;border-radius:0;box-shadow:none;background:none;}
.my-more-gongju .avpw_slider_handle{width:18px;height:18px;margin-left:-9px;margin-top:-9px;}

.my-more-gongju .my-brush-each.on{background:#4d4d4d;}

#my-apply, #my-save{position:absolute;right:25px;top:10px;background:#545454;color:#fff;font-weight:bold;cursor:pointer;text-align:center;width:84px;height:30px;line-height:30px;border-radius:15px;display:none;}
#my-save{display:block;}
.avpw_close_inner{background:#3E3E3E;}


#my-cbig li{padding-left:25px;margin-right:25px;background:url(../images/my/crop.png) no-repeat left center;}
#my-cbig #my-tool-crop{background-image:url(../images/my/crop.png);}
#my-cbig #my-tool-crop.on{background-image:url(../images/my/crop-active.png);}
#my-cbig #my-tool-orientation{background-image:url(../images/my/orientation.png);}
#my-cbig #my-tool-orientation.on{background-image:url(../images/my/orientation-active.png);}
#my-cbig #my-tool-resize{background-image:url(../images/my/resize.png);}
#my-cbig #my-tool-resize.on{background-image:url(../images/my/resize-active.png);}
#my-cbig #my-tool-effects{background-image:url(../images/my/effects.png);}
#my-cbig #my-tool-effects.on{background-image:url(../images/my/effects-active.png);}
#my-cbig #my-tool-text{background-image:url(../images/my/text.png);}
#my-cbig #my-tool-text.on{background-image:url(../images/my/text-active.png);}
#my-cbig #my-tool-drawing{background-image:url(../images/my/drawing.png);}
#my-cbig #my-tool-drawing.on{background-image:url(../images/my/drawing-active.png);}
#my-cbig #my-tool-more{background-image:url(../images/my/more.png);}
#my-cbig #my-tool-more.on{background-image:url(../images/my/more-active.png);}

.my-more-icon{background:url(../images/my/brightness.png) no-repeat;}
#my-tool-brightness .my-more-icon{background-image:url(../images/my/brightness.png);}
#my-tool-contrast .my-more-icon{background-image:url(../images/my/contrast.png);}
#my-tool-saturation .my-more-icon{background-image:url(../images/my/saturation.png);}
#my-tool-sharpness .my-more-icon{background-image:url(../images/my/sharpness.png);}
#my-tool-warmth .my-more-icon{background-image:url(../images/my/warmth.png);}
#my-tool-redeye .my-more-icon{background-image:url(../images/my/redeye.png);}
#my-tool-whiten .my-more-icon{background-image:url(../images/my/whiten.png);}
#my-tool-blemish .my-more-icon{background-image:url(../images/my/blemish.png);}


.my-effect-img{width:40px;height:40px;margin:0 auto 3px;background:url(../images/my/filters/original.png) no-repeat left top;background-size:40px 40px;}
.my-effect-original{background-image:url(../images/my/filters/original.png);}
.my-effect-singe{background-image:url(../images/my/filters/singe.png);}
.my-effect-purple{background-image:url(../images/my/filters/purple.png);}
.my-effect-aqua{background-image:url(../images/my/filters/aqua.png);}
.my-effect-edgewood{background-image:url(../images/my/filters/edgewood.png);}
.my-effect-bw{background-image:url(../images/my/filters/bw.png);}
.my-effect-softfocus{background-image:url(../images/my/filters/softfocus.png);}

#my-tool-orientation-content li{padding:38px 0 8px;background:url(../images/my/o-left.png) no-repeat center 8px;background-size:26px 26px;}
#my-tool-orientation-content li:hover{background-color:#686868;}
#my-tool-orientation-content .my-orientation-left{background-image:url(../images/my/o-left.png);}
#my-tool-orientation-content .my-orientation-right{background-image:url(../images/my/o-right.png);}
#my-tool-orientation-content .my-orientation-h{background-image:url(../images/my/o-h.png);}
#my-tool-orientation-content .my-orientation-v{background-image:url(../images/my/o-v.png);}


