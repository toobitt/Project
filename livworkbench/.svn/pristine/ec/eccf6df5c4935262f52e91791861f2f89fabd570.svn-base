html {
	position: relative;
	min-height: 500px;
}

#selectSettings {
	position: fixed;
	right: 0;
	top: 0;
	width: 360px;
	z-index: 10001;
	/*background: rgba(0, 0, 0, .7);*/
	color: #d8d8d8;
}
/*选择模式，显示*/
.selectMenu-model .select-cell-tabs {
	display: none;
}
.select-cell-tabs-model #selectMenu {
	display: none;
}
.select-cell-tabs-model {
	background: rgba(0, 0, 0, .7);
}
.select-cell-tab-item {
	display: none;
}
.select-cell-tab-item.current {
	display: block;
}

#selectSettings h3 {
	line-height: 40px;
	margin: 0 10px;
	border-bottom: 1px solid #888;
}
.select-cell-back {
	display: inline-block;
	background: url("../images/back.png") no-repeat 50% 50%;
	width: 26px;
	height: 26px;
	font-size:0;
	cursor: pointer;
}
/*关闭*/
.option-iframe-back{cursor:pointer;display:inline-block;position:absolute;right:0;width:34px;height:34px;margin:0;padding:0;background:url(../images/close.png) no-repeat center center,url(../images/form-button-gray.png) repeat-x;border-radius:2px;font-size:0;}
.option-iframe-back:hover{background:url(../images/close.png) no-repeat center center,url(../images/form-button-gray-hover.png) repeat-x;}

/*menu*/
#selectMenu {
	width: 100px;
	height: 100px;
	position: relative;
}	
.selectMenu-bg {
	width: 110px;
	height: 110px;
	position: absolute;
	left: -5px;
	top: -5px;
	z-index: -1;
	background: url("../images/ball.png");
	transition: all .5s;
}
.selectMenu-bg.small {
	transform: scale(.3);
}

.selectMenu-item-bg {
	fill: transparent;
}

/*attr*/
.select-attr-list {
	margin:0 10px;
	padding:0;
}
.select-attr-list li {
	border-bottom: 1px dotted #d8d8d8;
    display: block;
    padding: 15px;
}
.select-attr-list li:last-child {
	border-bottom:none;	
}
.select-attr-list input, .select-attr-list select {
	background: #696969;
	border: medium none;
    color: #D8D8D8;
    padding: 5px;
}
.common-button-group a {
    background-repeat: repeat-x;
    border-radius: 2px 2px 2px 2px;
    color: #FFFFFF;
    cursor: pointer;
    display: inline-block;
    height: 28px;
    line-height: 28px;
    margin-left: 5px;
    padding: 0 15px;
}
.common-button-group .blue {
    background-image: url("../images/small-blue.jpg");
}
.common-button-group .gray {
    background-image: url("../images/small-gray.jpg");
}
.common-button-group .blue:hover {
    background-image: url("../images/small-blue-hover.jpg");
}
.common-button-group .gray:hover {
    background-image: url("../images/small-gray-hover.jpg");
}

.select-cell-waiting {
	background: url("../images/loading2.gif") no-repeat 50% 50%;
	background-size: 30px 30px;
}
.select-cell-tabs {
	width: 375px;
}
.mode-list { 
	margin: 0px;
	overflow: auto; 
	padding: 20px; 
	height:400px;
}
.select-cell-tabs ul {
	height:400px;
	overflow: auto; 
}
.mode-list .ui-draggable-dragging {
	color: black;
}
.mode-list li {
	width: 90px;
	display: block;
	float: left;
	margin-right: 25px;
}
.mode-list li:nth-child(3n) {
	margin: 0;
}

.mode-list  span:first-child {
	width: 90px;
	height: 90px;
	display: block;
}
.mode-list li:hover span:first-child{
	margin: -1px;
	border: 1px solid #6ea5e8;
}
.mode-list li:hover span:last-child {
	color: #6ea5e8;
}
.mode-list  span:last-child {
	display: block;
	margin: 10px 0;
	text-align: center
}

#smallCellsView {
	position: absolute;
	left: 0;
	top: 0;
	width: 0;
	height: 0;
	background: rgba(255, 255, 0, 0);
	z-index: 10000;
	/*display: none;*/
}

.smallCellView {
	position: absolute;
	width: 100px;
	height: 100px;
	/* background: rgba(0, 0, 0, 0.5); */
}
.smallCellView:hover,.smallCellView.selected {
	border: 1px dashed red;
	margin-left: -1px;
	margin-top: -1px;
}
.smallCellView:hover{
	border-color: gray;
}
.smallCellView.selected {
	border-color: red;
}


.smallCellView.onhover {
	background: rgba(23, 100, 90, 0.4);
	transform: scale(1.5); 
	-webkit-transform: scale(1.5); 
}

.smallCellView.upother {
	/* -webkit-transform: scale(0.5); */
}

.livcms_cell {
	display: inline-block !important;
	width: 100px !important;
	height: 100px !important;
	line-height: 100px !important;
	margin: 20px !important;
	text-align: center !important;
	background: green !important;
	color: black !important;
}

/*模拟html的css*/
.mod-04 {
	width: 300px;
	display: inline-block;
}