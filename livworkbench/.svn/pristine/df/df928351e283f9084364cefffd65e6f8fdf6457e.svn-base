<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, minimal-ui">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="format-detection" content="telephone=no,email=no">
<title>徐州观音机场</title>
<link rel="stylesheet" href="./css/swiper.min.css">
<link rel="stylesheet" href="./css/common.css">
<link rel="stylesheet" href="./css/airport.css">
</head>
<body>
	<div class="swiper-container swiper-nav"></div>
	<div class="swiper-container swiper-content"></div>
<script type="text/html" id="content">
	<div class="swiper-wrapper">
		{{each list as value i}}
			<div class="swiper-slide">
				<div class="inner {{value.key}}" attr="{{value.key}}"></div>
			</div>
		{{/each}}
	</div>
</script>
<script type="text/html" id="list">
	{{if hasdata}}   
		<ul class="port-list">
		 {{each list as value ii}}   
		 	<li class="item {{value.mark}}">  
				<div class="info hg-flex">
					<div class="hg-flex-one">
						<h3>{{value.Flightnum}}</h3>
						<p class="city">{{value.City}}-{{value.Cityx}}</p>
					</div>
					{{if value.State}}<div class="status"><span class="tag">{{value.State}}</span></div>{{/if}}
				</div>
				<div class="column hg-flex">
					<div class="hg-flex-one {{if value.highlight == 'plan'}}highlight{{/if}}">
						<div class="label">计划</div><div class="time">{{value.P_time}}</div>
					</div>
					<div class="hg-flex-one {{if value.highlight == 'estimate'}}highlight{{/if}}">
						<div class="label">预计</div><div class="time">{{value.E_time}}</div>
					</div>
					<div class="hg-flex-one {{if value.highlight == 'actual'}}highlight{{/if}}">
						<div class="label">实际</div><div class="time">{{value.A_time}}</div>
					</div>
				</div>
		 	</li>   
		 {{/each}}  
		</ul>
 	 {{else}}   
		 <p class="nodata {{noport}}">{{tip}}</p>  
	 {{/if}}
</script>
<script type="text/html" id="html">
	{{if noport}}
		<p class="nodata noport">{{html}}</p>
	{{else}}
		<div class="airport_content">{{#html}}</div>
	{{/if}}
</script>
<script type="text/javascript" src="./js/module/head.min.js"></script>
<script type="text/javascript">
	head.js(
		{zepto : "./js/module/zepto.min.js"},
		{spin : "./js/module/spin.min.js"},
		{swiper : "./js/module/swiper.jquery.min.js"},
		{template : "./js/module/template.js"},
		{store : "./js/module/store.min.js"}
	);
	head.ready(["zepto", "swiper"], function(){
		head.js('./js/config.js', './js/index.js');
	});
</script>
</body>
</html>