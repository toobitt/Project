html{
    /*overflow-y:scroll;*/
    overflow-y:auto;
    overflow-x:hidden;
    background:url('../scripts/icons/page/page-body.png') repeat-y;
    padding:0;
    margin:0;
}
body{
    font-family:Verdana, Arial, Helvetica;
    font-size:14px;
    background-color:#fff;
    color:#000;
    line-height:1.5;
    width:641px;
    padding:15px 25px 10px;
    margin:0 0 15px;
    background:transparent;
    overflow:hidden;
}
