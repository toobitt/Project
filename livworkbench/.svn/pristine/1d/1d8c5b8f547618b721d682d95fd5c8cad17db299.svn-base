.pagebg{
    padding:50px 0 58px;
    margin:0 0 0 -25px;
    height:8px;
    display:block;
    border:none;
    cursor:default;
    clear:both;
}
.haspage{
    padding-top:58px;
    padding-bottom:50px;
}
.before-biaozhu, .after-biaozhu{position:relative;cursor:default;display:none;opacity:0;}
.before-biaozhu span, .after-biaozhu span{position:absolute;width:1px;height:25px;background:red;left:0px;top:-10px;}
.before-biaozhu span{left:-1px;}
/*.before-biaozhu-ok, .after-biaozhu-ok{position:relative;cursor:pointer;}
.before-biaozhu-ok span, .after-biaozhu-ok span{position:absolute;width:10px;height:25px;background:green;left:0px;top:-10px;font-size:12px;color:#fff;text-align:center;text-indent:0;line-height:25px;}
.before-biaozhu-ok span{left:-5px;}*/
.before-biaozhu-ok, .after-biaozhu-ok{position:absolute;cursor:pointer;margin-top:-10px;display:none;width:10px;}
.before-biaozhu-ok{margin-left:-10px;}

::selection{background:#f9cdcc;color:#000;}
::-moz-selection{background:#f9cdcc;color:#000;}
::-webkit-selection{background:#f9cdcc;color:#000;}

p{text-indent:2em;}

#word-uploading{font-size:12px;font-weight:normal;color:#dedede;background:url(../scripts/icons/slide/office.png) no-repeat;border:1px solid #dedede;height:53px;line-height:53px;padding-left:45px;margin:0 15px;}
#word-uploading span{color:#000;}

@media only screen and (-webkit-min-device-pixel-ratio: 2),
only screen and (-moz-min-device-pixel-ratio: 2),
only screen and (-o-min-device-pixel-ratio: 2/1),
only screen and (min-device-pixel-ratio: 2) {
	html{background-image:url('../scripts/icons/page/page-body-2x.png');background-size:1500px 4px;}
    #word-uploading{background-image:url(../scripts/icons/slide/office-2x.png);background-size:44px 44px;}
}
