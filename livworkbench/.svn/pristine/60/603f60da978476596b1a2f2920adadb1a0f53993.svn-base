/*.awaken{width:320px; margin:0 auto;}*/
body{background-color:#fff; }
.awaken{width:100%; background-color:#fff; padding-bottom:100px;}
.awaken header{border-bottom:1px solid #ccc; height:3.6em; line-height: 3.6em; background-color: #f8f9f8; }
.awaken h1{font-size:1.8em; text-align:center; }
.awaken .banner {overflow-x:auto; overflow-y:hidden; border-bottom: 1px solid #e9e9e9; margin:0 7px;}
.awaken .banner ul{width:100%; margin:9px 0;}
.awaken .banner li{float:left; width:110px; height:110px; margin:3px; font-size:0;  border:1px solid #ddd; border-radius:5px; }
.awaken .banner img{width:100%; height:100%; border-radius:4px;}
.awaken .content{border-bottom:1px solid #e9e9e9; padding:0 10px; }
.awaken .content > div{border-bottom:1px solid #e9e9e9; padding:8px 0;}
.awaken .content > div:last-child{border-bottom:none; }
.awaken .info h2{font-size:1.8em; }
.awaken .info h2 em{display:inline-block; width:22px; height:25px; background:url('images/sex_boy.png') no-repeat scroll center; }
.awaken .info h2 em.girl{ background-image:url('images/sex_girl.png'); }
.awaken .info h2 span{float:right; color:#390;}
.awaken .info > span{color:#808080; font-size:1.4em; }
.awaken .content li{float:left;height:24px; line-height:24px; font-size:1.4em;  }
.awaken .data li{width:100px;} 
.awaken .time span{margin-right:17px; }
.awaken .notime{color:#ddd; }
.awaken .call{text-align: center; margin:30px 0;}
.awaken .call a{display:inline-block; height:35px; line-height:35px; padding:0 30px; font-size:1.6em; color:#390; border:1px solid #390; border-radius:3px; }


@media only screen and (-webkit-min-device-pixel-ratio: 2),
only screen and (-moz-min-device-pixel-ratio: 2),
only screen and (-o-min-device-pixel-ratio: 2/1),
only screen and (min-device-pixel-ratio: 2) {
	.awaken .info h2 em{background-image:url('images/sex_boy-2x.png'); background-size:11px 12px; }
	.awaken .info h2 em.girl{ background-image:url('images/sex_girl-2x.png');  background-size:11px 12px; }
}

