<div class="m2o-each m2o-flex m2o-flex-center" data-id="{$v['id']}" orderid="{$v['order_id']}">    <div class="m2o-item m2o-paixu">		<input type="checkbox" value="{$v['id']}" title="{$v['id']}" name="infolist[]" class="m2o-check" style="visibility: visible;">	</div>		<div class="m2o-item m2o-flex-one m2o-bt" title="{$v['name']}">		<a class="common-title" href="./run.php?mid={$_INPUT['mid']}&a=form&id={$v['id']}&infrm=1" target="nodeFrame"  need-back>			<span class="m2o-item-bt">				{if $v['icon']}					{code}						$v['img']=hg_fetchimgurl($v['icon']);					{/code}					<img src="{$v['img']}"/>				{/if}				<span>{$v['fieldname']}</span>			</span>		</a>	</div>	<div class="m2o-item m2o-mark"><span>{$v['fieldmark']}</span></div>	<div class="m2o-item m2o-brief"><span>{$v['brief']}</span></div>			{code}				$addStatusType = array(					'0'=>'用户传值',					'1'=>'系统默认',					'2'=>'系统时间',					'3'=>'用户IP',				);				$addstatustitle = $addStatusType[(int)$v['addstatus']];			{/code}	  <div class="m2o-item m2o-mark"><span style="color:blue;">{$addstatustitle}</span></div>	  <div class="m2o-item m2o-mark">{if $v['issearch']}<span style="color:green;">是</span>{else}<span style="color:red;">否</span>{/if}</div>	  <div class="m2o-item m2o-mark">{if $v['isrequired']}<span style="color:green;">是</span>{else}<span style="color:red;">否</span>{/if}</div>	   <!--   <div class="m2o-item m2o-switch" _status="{$v['issearch']}" _type="issearch" >	    <span class="cover" {if $v['isrequired']}style="display:none;"{/if}></span>    	<div class="common-switch issearch {if $v['issearch']}common-switch-on{/if}">           <div class="switch-item switch-left" data-number="0"></div>           <div class="switch-slide"></div>           <div class="switch-item switch-right" data-number="100"></div>        </div>    </div>             <div class="m2o-item m2o-switch" _status="{$v['isrequired']}" _type="isrequired">    	<div class="common-switch isrequired {if $v['isrequired']}common-switch-on{/if}">           <div class="switch-item switch-left" data-number="0"></div>           <div class="switch-slide"></div>           <div class="switch-item switch-right" data-number="100"></div>        </div>    </div>    -->   <div class="m2o-item m2o-time">        <span class="name">{$v['user_name']}</span>        <span class="time">{$v['update_time']}</span>    </div>    <div class="m2o-item m2o-ibtn">    </div></div>