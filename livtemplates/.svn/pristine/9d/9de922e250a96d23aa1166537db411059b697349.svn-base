<li class="clear"  id="r_{$v['id']}"    name="{$v['id']}"   orderid="{$v['order_id']}"  onmouseout="hg_row_interactive(this, 'out');" onmouseover="hg_row_interactive(this, 'on');">	<span class="left">		{if $v['type']==4 && $v['id']!="1" && $v['id']!="2" && $v['id']!="3"}		<a class="lb" onclick="hg_row_interactive('#r_{$v[id]}', 'click', 'cur');"   name="alist[]" >			<input type="checkbox" name="infolist[]"  value="{$v['id']}" title="{$v['id']}"  />		</a>		{else}		<a style="color: #ccc;">&nbsp;内置</a>		{/if}	</span>	<span class="right" onclick="hg_row_interactive('#r_{$v[id]}', 'click', 'cur');" style="width: 850px">		{code}			$system_img = $v['system_img']['host'].$v['system_img']['dir'].$v['system_img']['filepath'].$v['system_img']['filename'];			$user_img = '';			if($v['user_img'])			{				$user_img = $v['user_img']['host'].$v['user_img']['dir'].$v['user_img']['filepath'].$v['user_img']['filename'];			}		{/code}		<a class="fl" style="width:80px;">{$v['pic_icon']}</a>        <a class="fl" style="width:80px;">{$v['pic_value']}</a>        <a class="fl" style="width:120px;color:#f00;">{if $v['is_on']==1}启用{else}未启用{/if}</a>        <a class="fl" style="width:120px;"><span id="user_img_{$v['id']}">{if $v['user_img']}<img src="{$user_img}" width="55" height="55"/>{else}&nbsp;{/if}</span></a>        <a class="fl" style="width:240px">{$v['user_name']} / {$v['update_time']}</a>        <span>        	<a title="编辑" href="./run.php?mid={$_INPUT['mid']}&a=form&id={$v['id']}&infrm=1">更新</a>        	        </span>	</span>	<span class="title overflow"  style="cursor:pointer;height:70px;">		<a id="report_{$v['id']}"><span class="m2o-common-title">{$v['title']}</span></a>	</span></li> 