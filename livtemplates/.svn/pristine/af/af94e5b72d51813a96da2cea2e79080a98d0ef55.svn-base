.confirm-model .record-edit,.play-model .record-edit,.info-model .record-edit{display:none;}
.confirm-model .record-edit-confirm{display:block;}
.play-model .record-edit-play{display:block;}
.info-model .record-edit-more-info{display:block;}

.record-edit-more-info{
	width: 360px;
	color: white;
}
.record-edit-more-info ul{
	overflow: hidden;
	padding: 20px 35px;	
}
.record-edit-more-info li{width:50%;float:left;line-height:30px;color:#aaa;border-bottom: 1px dotted #666;}
.record-edit-more-info li:last-child{border:none;}
.record-edit-more-info li span{color:#fff;margin-left:5px;}
.editor-hover{position:relative;}
.editor-hover:hover:after{position:absolute;content:'此操作会覆盖原视频';font-size:12px;text-align:left;text-indent:18px;background:url({$RESOURCE_URL}alert/o_delete.png) no-repeat;background-size:16px 16px;width:80px;line-height:1.4;top:10px;height:100px;left:0;top:36px;}
/*****************************weight-select***********************************/
#weight_box.box-updown{margin-top:-285px;}
#weight_box.box-updown .arrow{top:auto;bottom:-9px;transform:rotate(180deg);-webkit-transform:rotate(180deg);}

.common-switch-status{cursor:pointer;position:relative;}
#weight_box{display:none;position:absolute;z-index:10;margin-left:-65px;}
#weight_box li{line-height:30px;cursor:pointer;}

@media only screen and (-webkit-min-device-pixel-ratio: 2),
only screen and (-moz-min-device-pixel-ratio: 2),
only screen and (-o-min-device-pixel-ratio: 2/1),
only screen and (min-device-pixel-ratio: 2) {

	.common-list div.common-list-i{background-image:url({$RESOURCE_URL}common/icon_info-2x.png);background-size:12px 12px;}
    #open-close-box{background-image:url({$RESOURCE_URL}common/open-close-2x.png);background-size:15px 14px;}
    #open-close-box:hover{background-image:url({$RESOURCE_URL}common/open-close-hover-2x.png);} 
	.common-quanzhong{background-image:url({$RESOURCE_URL}common/quanzhong-2x.png) ;background-size:168px 48px;}
	.common-quanzhong-option .common-quanzhong-down{background-image: url("{$RESOURCE_URL}common/common-quanzhong-down-2x.png")  ;background-size:8px 8px;}
	.common-quanzhong-option .common-quanzhong-up{background-image: url("{$RESOURCE_URL}common/common-quanzhong-up-2x.png")  ;background-size:8px 8px;}
	.common-quanzhong-box0 .common-quanzhong-down{background-image:url("{$RESOURCE_URL}common/common-quanzhong-down-off-2x.png");background-size:8px 8px;}
	.common-quanzhong-box12 .common-quanzhong-up{background-image:url("{$RESOURCE_URL}common/common-quanzhong-up-off-2x.png") ;background-size:8px 8px;}
	.weight-select .quanzhong-list li{background-image:url({$RESOURCE_URL}/common/qz_libg-2x.png);background-size:100%;}
	.weight-select .arrow{background-image:url({$RESOURCE_URL}/common/qz_new_arrow-2x.png);background-size:100%;}
}
