{template:./head_member}{css:manage}{js:My97DatePicker/WdatePicker}{code}//print_r($member_infos);{/code}<style>#wrap{background:white;border-radius:3px}.nav {height: 30px; margin: 10px 0px 0px;padding-left:10px;padding-top:8px}.nav li {color: #666666;display: inline; float: left; font-size: 14px;line-height: 24px;margin-right: 20px;}.nav #goto_index {float: right;}.nav_con {line-height:20px;}.nav_con li{padding:0 0 0 20px}.nav_con input{border:1px solid #DDD;padding:3px 5px;}#actionForm label{width:70px;display:inline-block;}#actionForm div{margin-bottom:20px}.add_group_btn input{background:url({$RESOURCE_URL}qingao/add2.png) no-repeat;width:122px;height:55px;margin-top:20px;margin-left:10px;line-height:55px;color:#FFF;font-size:18px;border:none;cursor:pointer;}</style><div id="wrap" class="wrap"><ul class="nav">	<li  class="currentSetting">{$owner}的资料</li>	<li><a href="member.php?a=editsign&uid={$uid}">{$owner}的兴趣</a></li>	<li id="goto_index"><a href="member.php?uid={$uid}">返回{$owner}的主页</a></li></ul><form action="member.php?a=doedit" method="post" enctype="multipart/form-data" id="actionForm"><div class="clear"></div><div>	<label>昵称:</label>	<input type="text" value="{$member_infos['nick_name']}" name="q_nick_name" size="20"  disabled='disabled'/></div><div class="clear"></div><div>	<label>邮件:</label>	<input type="text" value="{$member_infos['email']}" name="q_email" size="40" {if $owner !="我"} disabled='disabled'{/if}/></div><div class="clear"></div><div><label>性别:</label>	<select name="q_sex" {if $owner !="我"} disabled='disabled'{/if}> 		<option value="0" {code}echo ($member_infos['sex']==0)? "selected" : ""; {/code} >保密</option>		<option value="1" {code}echo ($member_infos['sex']==1)? "selected" : ""; {/code} >男</option>		<option value="2" {code}echo ($member_infos['sex']==2)? "selected" : ""; {/code} >女</option>	</select></div><div class="clear"></div><div><label>血型:</label>	<select name="q_bloodtype" {if $owner !="我"} disabled='disabled'{/if}> 		<option value="1" {code}echo ($member_infos['bloodtype']==1)? "selected" : ""; {/code} >A型</option>		<option value="2" {code}echo ($member_infos['bloodtype']==2)? "selected" : ""; {/code} >B型</option>		<option value="3" {code}echo ($member_infos['bloodtype']==3)? "selected" : ""; {/code} >O型</option>		<option value="4" {code}echo ($member_infos['bloodtype']==4)? "selected" : ""; {/code} >AB型</option>		<option value="5" {code}echo ($member_infos['bloodtype']==5)? "selected" : ""; {/code} >其它</option>	</select></div><div class="clear"></div><div>	<label>生日:</label>	<input id="d421" name="q_birth" value="{$member_infos['birth']}" class="Wdate" type="text" onfocus="WdatePicker({skin:'whyGreen',maxDate:'%y-%M-%d'})" {if $owner !="我"} disabled='disabled'{/if}/></div><div class="clear"></div><div>	<label>QQ:</label>	<input type="text" value="{$member_infos['qq_num']}" name="q_qq_num" size="30" {if $owner !="我"} disabled='disabled'{/if}/></div><div class="clear"></div><div>	<label>手机:</label>	<input type="text" value="{$member_infos['mobile']}" name="q_mobile" size="30" {if $owner !="我"} disabled='disabled'{/if}/></div><div class="clear"></div><input type="hidden" name="q_member_id" value="{$member_infos['id']}" id="group_addr" />{if $owner =="我"}<div class="add_group_btn">	<input type="submit" name="add_group_btn" value="保存" /></div>{/if}</from></div>{template:./footer}