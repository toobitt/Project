.fullcalendar-wrapper .fc-content{color:#737373; }
.fullcalendar-wrapper .fc-content th{height:38px; line-height:38px; font-size:12px; font-weight:normal; background-color:#f9f9f9; }
.fullcalendar-wrapper .fc-widget-header, .fc-widget-content{border-color:#e0e0e0; }

.fullcalendar-wrapper.fc-ltr .fc-event{border-radius:0; }
1.fullcalendar-wrapper.fc-ltr .fc-event:hover{background-color:#f0f2fe; color:#737373; }
.fullcalendar-wrapper .fc-event-inner{line-height: 16px; }

1.fullcalendar-wrapper .fc-grid .fc-day-number{position:relative; z-index: 1001;}
1.fullcalendar-wrapper .fc-widget-content{position:relative; }
1.fullcalendar-wrapper .fc-widget-content:not(.fc-other-month):hover:after{position:absolute; left:-1px; top:-1px; width:100%; height:100%; line-height:84px; color:#5b6fce; font-size:12px; z-index:999; text-align:center; border:1px solid #5b6fce; background-color:#f0f2fe; content:"添加排班"; }
.fullcalendar-wrapper .fc-widget-content:not(.fc-other-month):hover{background-color:#f0f2fe; }
