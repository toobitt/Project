<ul class="scrollUl fz">	<li class="t01" id="n11">{$owner}参加的<span>|</span>	</li>	<li class="t02" id="n12">{$owner}管理的<span></span>	</li></ul><hr class="hr_lv"><div class="bor03 ">	<div id="f11">	{if $join_group}		<ul class="my_group_list" id="group_atten_list">			{foreach $join_group as $v}			<li><a href="group.php?group_id={$v['group_id']}"><img					src="{if is_string($v['logo'])}{$v['logo']}{else}{$v['logo']['host']}{$v['logo']['dir']}102x102/{$v['logo']['filepath']}{$v['logo']['filename']}{/if}">			</a>				<p>					<a href="group.php?group_id={$v['group_id']}">{$v['name']}</a>				</p></li> {/foreach}		</ul>		{if $join_total}		<div class="gtalk_more moreCon" attr="group_atten" total="{$join_total}">点击获取更多信息...</div>		{/if}	{else}	<div>暂没有信息</div>	{/if}	</div>	<div id="f12" class="hidden">	{if $create_group}		<ul class="my_group_list" id="group_host_list">			{foreach $create_group as $v}			<li><a href="group.php?group_id={$v['group_id']}"><img					src="{if is_string($v['logo'])}{$v['logo']}{else}{$v['logo']['host']}{$v['logo']['dir']}102x102/{$v['logo']['filepath']}{$v['logo']['filename']}{/if}">			</a>				<p>					<a href="group.php?group_id={$v['group_id']}">{$v['name']}</a>				</p></li> {/foreach}		</ul>		{if $create_total}		<div class="gtalk_more moreCon" attr="group_host" total="{$create_total}">点击获取更多信息...</div>		{/if}	{else}	<div>暂没有信息</div>	{/if}	</div></div><script type="text/javascript">$(function() {	var SD3 = new scrollDoor();	SD3.sd(["n11","n12"],["f11","f12"],"t01","t02");	var page = 2;	$('.moreCon').click(function() {		var _this = $(this);		var _v = _this.html();		var method = _this.attr('attr');		var total = _this.attr('total');		$.ajax({			type : 'post',			url : 'member.php',			data : 'a=get_group&uid='+uid+'&total='+total+'&method='+method+'&p='+page+'&ajax=1',			cache : false,			async : true,			dataType : 'json',			success : function(data) {				var result = eval(data.callback);				if (page < result.totalPages)				{					page++;					$('#'+method+'_list').append(result.html);					_this.html(_v);				}				else				{					$('#'+method+'_list').append(result.html);					_this.remove();				}			},			beforeSend : function() {				_this.html('<br /><img src="'+RESOURCE_URL+'loading.gif" />');			}		});	});	function hg_call_member(data, total)	{		return {			html : data,			totalPages : total		};	}});		</script>