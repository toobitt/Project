#stream-box{position:relative;}
#shiyi-box{width:100%;height:100%;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    position:absolute;left:0;top:0;z-index:100;overflow:hidden;
}
.shiyi-cat{border-top:1px solid #454545;}
.shiyi-cat li{cursor:pointer;border-bottom:1px solid #454545;height:30px;line-height:30px;position:relative;padding-left:10px;color:#a3a3a3;}
.shiyi-cat .on{color:#fff;}
.shiyi-cat .on:after{
    content:'';
    position:absolute;
    right:10px;
    top:11px;
    width:7px;
    height:10px;
    background:url({$RESOURCE_URL}schedule/sanjiao.png) no-repeat;
}
.shiyi-content-outer{overflow:hidden;}
.shiyi-content{overflow:auto;height:500px;margin-left:30px;cursor:default;}
.shiyi-left{float:left;width:125px;}
.shiyi-back{margin-bottom:50px;}
.shiyi-back-all, .shiyi-current{height:37px;line-height:37px;font-size:14px;padding-left:28px;}
.shiyi-back-all{background:#4d4d4d;margin-bottom:1px;position:relative;cursor:pointer;}
.shiyi-back-all:before{
    content:'';
    position:absolute;
    left:10px;
    top:12px;
    width:16px;
    height:13px;
    background:url({$RESOURCE_URL}schedule/back.png) no-repeat;
}
.shiyi-current{background:#5a98d1;}
.shiyi-list-item{display:none;height:500px;overflow:auto;}
.shiyi-list-item.on{display:block;}
.shiyi-list-item ul, .shiyi-helper{padding:0 10px;margin-bottom:5px;background:#4d4d4d;}
.shiyi-list-item li{height:35px;line-height:35px;border-bottom:1px solid #303030;padding:0 10px;}
.shiyi-list-item ul li:last-child{border-bottom:none;}
.shiyi-time{margin-right:20px;}
.shiyi-helper{height:35px;line-height:35px;color:#fff;width:380px;position:absolute;left:0;top:0;}

#stream-box{
    -webkit-perspective: 0;
    -webkit-transform-style: preserve-3d;
    -webkit-backface-visibility: hidden;

    -moz-perspective: 0;
    -moz-transform-style: preserve-3d;

    perspective: 0;
    transform-style: preserve-3d;
}
#pindao-box{
    background-color:#303030;
    -webkit-transition-property: -webkit-transform;
    -webkit-transition-duration: 0.5s;

    -moz-transition-property: -moz-transform;
    -moz-transition-duration: 0.5s;

    transition-property: transform;
    transition-duration: 0.5s;
}
#shiyi-box{
    background-color:#303030;
    -webkit-transform: translateX(230px) translateZ(-230px) rotateY(90deg);
    -webkit-transition-property: -webkit-transform;
    -webkit-transition-duration: 0.5s;

    -moz-transform: translateX(230px) translateZ(-230px) rotateY(90deg);
    -moz-transition-property: -moz-transform;
    -moz-transition-duration: 0.5s;

    transform: translateX(230px) translateZ(-230px) rotateY(90deg);
    transition-property: transform;
    transition-duration: 0.5s;
}
#stream-box.hover #pindao-box{
    -webkit-transform: translateX(-230px) translateZ(-230px) rotateY(-90deg);
    -moz-transform: translateX(-230px) translateZ(-230px) rotateY(-90deg);
    transform: translateX(-230px) translateZ(-230px) rotateY(-90deg);
}
#stream-box.hover #shiyi-box{
    -webkit-transform: translateX(0px) translateZ(0px) rotateY(0deg);
    -moz-transform: translateX(0px) rotateY(0deg);
    transform: translateX(0px) rotateY(0deg);
}



