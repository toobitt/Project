$(function() {	$("#upload_img").click(function() {		// 上传方法		$.upload({			// 上传地址			url: 'activitys.php', 			// 文件域名字			fileName: 'Filedata', 			// 其他表单数据			params: {a: 'material'},			// 上传完成后, 返回json, text			dataType: 'json',			// 上传之前回调,return true表示可继续上传			onSend: function() {				return true;			},			// 上传之后回调			onComplate: function(data) {				var html = '<img src="'+data.host+data.dir+'626x290/'+data.filepath+data.filename+'" width="626" height="290" />';				$('#show_img').html(html);				$('input[name="id"]').val(data.id);				$('input[name="host"]').val(data.host+data.dir);				$('input[name="filepath"]').val(data.filepath+data.filename);			}		});	});});