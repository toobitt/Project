.task_live .program .tv .last_time {width:2%;padding-left:0;}


/*时间轴*/
.live_time{margin:0px auto 0;background:url("{$RESOURCE_URL}live_bg_2.png") left bottom repeat-x;height:62px;width:100%;left:0}
.live_time_2{background:url("{$RESOURCE_URL}live_bg.png") left bottom repeat-x;}
.live_time .middle{height:62px;min-width:120px;width:500px; margin:0 auto}
.live_time .middle .middle_top{height:44px;border:3px solid #bd0608;position:relative;z-index:90}
.zoom .middle_top{width:240px;border:3px solid #bd0608;position:absolute;height:55px;top:7px;left:0;background:#ffa59e;line-height:12px;filter:alpha(opacity=60);-moz-opacity:0.6;opacity: 0.6;}
.middle_top .green,.middle_top .yellow{width:17px;height:24px;background:url("{$RESOURCE_URL}list-all.png")  no-repeat;position:absolute;bottom:-25px;display:block; cursor:pointer; z-index:97}
.live_time .middle .middle_top .green{background-position:-4px -194px;left:-9px;}
.live_time .middle .middle_top .yellow{background-position:-24px -194px;right:-9px;}
#box_2{ cursor:pointer;}
/*放大*/
.zoom{height:75px;width:400px;background:url("{$RESOURCE_URL}live_move.png") repeat-x;padding:0px 5px;position:absolute; margin:0 auto 0;top:-10px;left:0}
.zoom a.green,.zoom a.yellow{height:19px;width:19px;position:absolute;bottom:-5px;display:block; cursor:pointer; z-index:101;background:url("{$RESOURCE_URL}list-all.png") no-repeat;}
.zoom a.green{background-position:-3px -219px;left:-5px;}
.zoom a.yellow{background-position:-25px -219px;right:-10px}
.zoom ul{margin:7px 0 0px;padding-top:3px}
.zoom ul li{ float:left;margin:0 1px 0px 0px}
#zoom_none{ position:fixed;height:200%;width:100%;z-index:99;top:0;left:0}
#zoom_big{display:none;position:absolute;top:0;left:0;right:0;bottom:0;height:100%;width:100%;z-index:90;_top:expression(eval(document.compatMode &&document.compatMode=='CSS1Compat')?document.documentElement.scrollTop:document.body.scrollTop +(document.body.clientHeight-this.clientHeight));}

/*视频*/
.video,.video_click{width:260px;height:196px;margin-bottom:10px;cursor:pointer;border:3px solid #000;-moz-border-radius:5px;-webkit-border-radius: 5px;border-radius:5px;position:relative;z-index:100;top:0;left:0}
.video{background:url("{$RESOURCE_URL}../IMG/9.jpg");}
.video_click{background:url("{$RESOURCE_URL}../IMG/8.jpg");}
.revision{position:absolute;bottom:3px;width:98%; margin:0 auto;height:19px;left:3px;}
.revision .time{width:110px;height:19px; text-align:center; position:absolute;left:0;bottom:0;background:#2c2e3a}
.revision .time span{color:#EFEFEF;font-size:12px;line-height:19px;}
.revision .time a{display:inline-block;font-size:0;line-height:0;margin-top:0;width:18px;height:19px;}
.revision .time .left{float:left; background:url("{$RESOURCE_URL}list-all.png") -3px -242px no-repeat;}
.revision .time .right{float:right; background:url("{$RESOURCE_URL}list-all.png") -24px -242px no-repeat;}
#preview{position:absolute;bottom:0;right:0;color:#fff;line-height:17px;border-radius:2px; background:#000;width:36px;height:17px;border:1px solid #141519; background:url("{$RESOURCE_URL}preview.gif") repeat-x; text-align:center;display:block}
/*live_menu*/
.live_menu{width:100%; font-family:Arial, Helvetica, sans-serif;}
.live_menu dl{padding:8px 18px;line-height:41px;display:inline-block;width:97%;background:url("{$RESOURCE_URL}live_menu_bg.png") repeat-x;height:41px;}
.live_menu dl dd.font_none{font-weight:100;}
.live_menu dl dt,.live_menu dl dd{ float:left;margin:0 2px;font-size:14px;color:#515151;font-weight:bold;}
.live_menu dl dd.switch{margin-right:10px;}
.live_menu dl dd.switch a{color:#145AA2;font-size:12px;font-weight:100; text-decoration:underline}
.live_menu dl dd.botton_sc,.live_menu dl dd.botton_fz{float:right;font-size:0; text-indent:999px;overflow:hidden; background:url("{$RESOURCE_URL}live_menu.png") no-repeat;height:26px;}
.live_menu dl dd.botton_sc{ background-position:-72px -2px;width:81px;}
.live_menu dl dd.botton_fz{ background-position:-2px -2px;width:68px;}
.live_menu .live_menu_map{margin-top:10px; clear:both}
.live_menu .left{width:80px;border:1px solid #bfbdbe; border-right:0;float:left;display:inline; margin:37px 0 0 17px;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;box-shadow:0px 0px 6px #ccc; background:#fff }
.live_menu .right{width:85%;border:3px solid #656565;float:left;margin:7px 0 0 0px;height:344px;-webkit-transition-property:color,background-color,padding-left;-webkit-transition-duration:500ms,500ms,500ms;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;box-shadow:0px 0px 6px #ccc;-webkit-box-shadow:0px 0px 6px #ccc;overflow-x:scroll}
.live_menu .left li{line-height:39px;padding:0 14px 0 8px;border-bottom:1px solid #bebebe}
.live_menu .left li:last-child{border:0;}
.live_menu .left li a{display:block}
.live_menu .left li span{float:right;}
.live_menu .left li.hover a{color:#373737; font-weight:bold;}
.live_menu .right .time li{float:left;width:4%;font-size:14px;padding-left:8px;line-height:27px;}
.live_menu .right .time{width:441%;background:url("{$RESOURCE_URL}live_menu_title.png") repeat-x;height:27px;border-bottom:1px solid #9a9a9a;border-top:1px solid #fff}
.jiemu {width:441%;display:table}
.jiemu li{border-top:1px solid #fff;border-right:1px solid #d2d2d2;border-bottom:1px solid #9a9a9a;float:left;width:4%;font-size:12px;line-height:25px;padding-left:8px;}
.blue{background:#dcf3fb}
.blue a{color:#49a349}
.green{background:#e3fbe3;color:#2689b2}
.green a{color:#2689b2}
.pink{background:#feeaeb;color:#e46465}
.pink a{color:#e46465}
.jiemu li span{color:#9d9b9c;display:block;height:12px;line-height:22px;}

/*CSS3效果*/
.zoom{opacity: 1;-webkit-transition: opacity 1s linear;}
.zoom_2{opacity:0;display:none\9;*display:none;_display:none}
/*圆角/阴影*/
.pub_div_bg,.part #column .recent ul{-moz-border-radius:3px;-webkit-border-radius:3px;border-radius:3px;}
.pub_div_bg,.bg_middle .add ul li a{-moz-box-shadow:1px 1px 4px #ccc;-webkit-box-shadow:1px 1px 4px #ccc;box-shadow:1px 1px 4px #ccc;}
body.ie .content{bckground:none;border:0;padding:0 0 0;box-shadow:none;}
body.ie .content_left{width:15px;min-height:900px;background:url("{$RESOURCE_URL}conten_left.png") repeat-y;float:left;display:inline;}
body.ie .content_right{width:15px;min-height:900px;background:url("{$RESOURCE_URL}conten_right.png") repeat-y;float:right;}
body.ie .content_middle{margin:0 auto;padding:20px 0 0;width:97.2%;}
.content_middle_1{width:99%;height:62px; margin:0 auto; position:relative;}
.content_middle_2{width:990px;left:50%;float:left; position:relative;margin-left:-495px;margin-top:10px;}
body.ie .content_middle_0{width:98.2%;margin:0 auto}
body.ie .top .live table.bor{width:99.5%;background:#fff;border:1px solid #d8d8d8}
/*已标注*/
.biaoz{min-width:1024px;}
.hidden{display:none; text-indent:0;clear:both;font-size:0;line-height:0;}
.biaoz .content{padding:0 0 10px;border:1px solid #d8d8d8;border-radius:5px;}
.been_marked{width:137px; background:#fff;border-top-left-radius:5px;float:left}
.been_marked li a{position:relative;font-weight:bold;display:block;line-height:39px;height:39px;text-indent:20pt;border-bottom:1px solid #ebebeb;}
.been_marked li a:hover{background:#e6f3fc;line-height:39px;}
.been_marked li.check a:hover{background:none;}
.been_marked li ul li{text-indent:25pt}
.been_marked li ul li a{font-weight:normal;line-height:26px;height:26px;text-indent: 26pt;}
.been_marked li ul li a:hover{line-height:26px;}
li.check,li:hover.check{ background:url("{$RESOURCE_URL}li_bg.png") repeat-x}
li.check a{color:#fff}
a:first.check{border-top-left-radius:6px;}
.biaoz em,.biaoz strong{display:inline-block;font-size:0;line-height:0;background:url("{$RESOURCE_URL}bg-all.png") no-repeat}
.been_marked li em{height:10px;width:7px;background-position:-102px -8px;position:absolute;top:14px;left:15px;}
li.check em{background-position:-100px -28px;width:10px;height:9px;top:16px;left:13px;}
li.check_none em{background-position:-102px -8px;top:14px;}
stong{height:13px;width:13px;background:url("{$RESOURCE_URL}bg-all.png") -102px -8px no-repeat}
.color_green a,.color_green{color:green;font-weight:bold}
li.check li a{color:green;}
.color_blue a,.color_blue{color:#1099bb;font-weight:bold}
.color_yellow a,.color_yellow{color:#be8243;font-weight:bold}
.color_zs a,.color_zs{color:#804dcc;font-weight:bold}
.been_marked li strong{display:inline-block;font-size:0;line-height:0;background:url("{$RESOURCE_URL}bg-all.png") no-repeat; float:right;margin:14px 40px 0 0;}
.been_marked li.sy strong{background-position:-22px -5px;width:13px;height:13px;margin:14px 40px 0 0;}
.been_marked li.sc strong{background-position:-22px -25px;width:15px;height:13px;margin:14px 38px 0 0;}
.been_marked li.jh strong{background-position:-3px -4px;width:14px;height:12px;margin:14px 38px 0 0;}

/*弹出框CSS*/
#boxBg {
	filter:alpha(opacity=50);
	opacity:0.5;
	background:#000;
}
.boxLoading,.boxError {
	position: absolute;
	display: block;
	width: 90px;
	height: 30px;
	line-height: 30px;
	margin-left: -45px;
	margin-top: -15px;
	left: 50%;
	top: 50%;
	color: #f00;
}
.boxLoading{
	width:28px;
	margin-left: -14px;
	background:url("{$RESOURCE_URL}loading.gif") no-repeat;
	text-indent: -999em;
	text-align: center;
}
.boxTitle {
	position: relative;
	border: 1px solid #A6C9E1;
	background: url("{$RESOURCE_URL}ico.png") 0 0 repeat-x;
	height: 30px;
	line-height: 30px;
}
.boxTitle h3 {
	float: left;
	font-weight: normal;
	color: #666;
	font-size: 14px;
	margin: 0;
	text-indent: 10px;
}
.boxTitle span {
	position: absolute;
	width: 10px;
	background: url("{$RESOURCE_URL}ico.png") -80px -40px no-repeat;
	text-indent: -10em;
	right: 10px;
	top: 10px;
	height: 16px;
	overflow: hidden;
	cursor: pointer;
}
.boxTitle span.hover {
	background: url("{$RESOURCE_URL}ico.png") -90px -40px no-repeat;
}
.boxContent {
	padding:0px;
	border: 1px solid #A6C9E1;
	border-top: none;
	text-align: center;
	overflow: auto;
}
.shadow {
	box-shadow:3px 3px 5px #bfbfbf;
	-moz-box-shadow: 3px 3px 5px #bfbfbf;
	-webkit-box-shadow:3px 3px 5px #bfbfbf;
}
#drag7 div
{
	cursor: move;
	background-color: #900;
	height: 15px;
}
