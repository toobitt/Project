{if $owner && $owner == '我'}<ul class="scrollUl fz">	<li class="t01" id="create_albums_btn">创建相册</li></ul><hr class="hr_lv">{else}<hr class="hr_pic_lv">{/if}<ul class="my_pic_list" id="pic">	{foreach $albums_info as $v}	<li id="albums_{$v['albums_id']}">		<a href="javascript:;" albumsid="{$v['albums_id']}"><img src="{if $v['sys']}{$RESOURCE_URL}{$v['cover_file_name']}{else}{if is_string($v['cover_file_name'])}{$v['cover_file_name']}{else}{$v['cover_file_name']['host']}126x126/{$v['cover_file_name']['filepath']}{/if}{/if}" /></a>		<p><a href="javascript:;" albumsid="{$v['albums_id']}">{$v['albums_name']}（{$v['picture_count']}张）</a></p>		{if ($_user['id'] == $v['user_id']) && $v['sys'] != 1}		<p><a href="javascript:;" m="drop_albums" albumsid="{$v['albums_id']}">删除</a></p>		{/if}	</li>	{/foreach}</ul>{if $more_imgs}<div class="gtalk_more moreCon" style="clear:both;" total="{$albums_count}">点击获取更多信息...</div>{/if}<style><!--#create_albums {clear:both; border:1px solid #CCC; padding:10px; display:none;}#create_albums ul li {margin:10px 0;}#create_albums label {vertical-align:top;}#loading {display:none;}#pic {overflow:visible;}--></style><div id="create_albums">	<form method="post" enctype="multipart/form-data" onsubmit="javascript:return false;" id="albums_form">		<ul>			<li><label>相册名称：</label><input type="text" name="albums_name" id="albums_name" size="40" /></li>			<li><label>相册描述：</label><textarea cols="40" rows="4" name="albums_desc" id="albums_desc"></textarea></li>			<li><label>相册封面：</label><em id="show_img"><img src="{$default_albums}" width="126" height="126" /></em>			<p style="margin-left:80px;"><input id="upload_img" value="上传封面" type="button" /></p></li>			<li><input type="button" name="albums_btn" id="albums_btn" value="创建相册" /></li>		</ul>	</form></div><div id="loading"></div><script type="text/javascript">$(function() {	$('#pic a').live('click', function() {		var albumsid = $(this).attr('albumsid');		var method = $(this).attr('m');		if (!albumsid) return;		if (!method) {			$.ajax({				type : 'post',				url : 'member.php',				data : 'a=photo&uid='+uid+'&albumsid='+albumsid,				cache : false,				async : true,				dataType : 'html',				success : function(data) {					$('#tabCon').html(data);				},				beforeSend : function() {					$('#pic').html('<br /><img src="'+RESOURCE_URL+'loading.gif" />');				}			});		}else {			if (confirm('您真的要删除该相册吗？')) {				$.getJSON('member.php?a='+method+'&albumsid='+albumsid, function(data) {					if (data.error) alert(data.error);					if (data.msg) {						$('#albums_'+albumsid).remove();					}				});			}		}	});		var con = '<form method="post" enctype="multipart/form-data" onsubmit="javascript:return false;" id="albums_form"><ul><li><label>相册名称：</label><input type="text" name="albums_name" id="albums_name" size="40" /></li><li><label>相册描述：</label><textarea cols="40" rows="4" name="albums_desc" id="albums_desc"></textarea></li><li><label>相册封面：</label><em id="show_img"><img src="{$default_albums}" width="126" height="126" /></em><p style="margin-left:80px;"><input id="upload_img" value="上传封面" type="button" /></p></li><li><input type="button" name="albums_btn" id="albums_btn" value="创建相册" /></li></ul></form>';		$('#create_albums_btn').click(function() {		$('#create_albums').toggle('slow');	});	$("#upload_img").click(function() {		// 上传方法		$.upload({			// 上传地址			url: 'member.php', 			// 文件域名字			fileName: 'Filedata', 			// 其他表单数据			params: {a: 'albums_cover'},			// 上传完成后, 返回json, text			dataType: 'json',			// 上传之前回调,return true表示可继续上传			onSend: function() {				return true;			},			// 上传之后回调			onComplate: function(data) {				var html = '<img src='+data.host+data.dir+'126x126/'+data.filepath+data.filename+' width="126" height="126" />';				$('#show_img').html(html);								$('<input>', {					'type' : 'hidden',					'name' : 'id',					'value' : data.id				}).appendTo($('#albums_form'));								$('<input>', {					'type' : 'hidden',					'name' : 'host',					'value' : data.host+data.dir				}).appendTo($('#albums_form'));								$('<input>', {					'type' : 'hidden',					'name' : 'filepath',					'value' : data.filepath+data.filename				}).appendTo($('#albums_form'));			}		});	});		$('#albums_btn').click(function() {		var albums_data = $('#albums_form').serialize();		$.ajax({			type : 'post',			url : 'member.php',			data : 'a=create_albums&'+albums_data+'&ajax=1',			cache : false,			async : true,			dataType : 'json',			success : function(data) {				if (data.msg)				{					alert(data.msg);				}				else				{					var result = eval(data.callback);					if (result.html) {						$('#pic').append(result.html);					}					$('#loading').hide();				}			},			beforeSend : function() {				$('#create_albums').html(con);				$('#create_albums').hide();				$('#loading').html('<br /><img src="'+RESOURCE_URL+'loading.gif" />');				$('#loading').show();			}		});	});	function hg_call_albums(data, total)	{		return {			html : data,			totalPages : total		};	}	function hg_show_error(error)	{		//alert(error);		$('#loading').hide();	}	var page = 2;	$('.moreCon').click(function() {		var _this = $(this);		var _v = $(this).html();		var total = _this.attr('total');		$.ajax({			type : 'post',			url : 'member.php',			data : 'a=get_more_albums&uid='+uid+'&total='+total+'&p='+page+'&ajax=1',			cache : false,			async : true,			dataType : 'json',			success : function(data) {				var result = eval(data.callback);				if (page < result.totalPages)				{					page++;					$('#pic').append(result.html);					_this.html(_v);				}				else				{					$('#pic').append(result.html);					_this.remove();				}			},			beforeSend : function() {				_this.html('<br /><img src="'+RESOURCE_URL+'loading.gif" />');			}		});	});});</script>