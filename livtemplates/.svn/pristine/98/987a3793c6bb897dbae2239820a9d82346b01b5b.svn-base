.form_ul .info{line-height:35px;}
.form_ul .info input{width: 474px;height: 25px!important;text-align: left;font-family: 'Arial';font-size: 13px;color: #000000;font-weight: normal;font-style: normal;text-decoration: none;}
.form_ul textarea{width:474px;}
.type-choose{margin-left: -20px;margin-top: -7px;}
.type-choose li{float:left;margin: 10px 0px 0px 20px;}
.type-choose li span{margin-left: 10px;}
.default-option{width:484px;margin-left: 82px;margin-bottom: 12px;display:none;}
.default-option p{cursor: pointer;width: 20px;height: 20px;
background: #7bb0e6;display: inline-block;border-radius: 10px;text-align: center;color: #fff;margin: 6px 0 0 15px;font-size: 20px;line-height: 17px;}
.option-contain input{text-indent: 5px;margin:4px 0 4px 3px;width:109px;font-size:14px;}
.temp-edit-buttons{padding-bottom:20px;}
.del-extend{height:20px; width:20px;margin-left:10px;display:none; background:url("{$RESOURCE_URL}tuji/cc.png") no-repeat center center #fff; cursor:pointer;position: absolute;bottom: 6px;right: 11px;}
.icon:hover .del-extend{display:inline-block; }
.del-creditrules{height:20px; width:20px;margin-left: 20px;margin-top: -34px;display:none; background:url("{$RESOURCE_URL}tuji/cc.png") no-repeat center; cursor:pointer;}
.rules_del:hover .del-creditrules{display:inline-block; }
.medal-list li{width: 120px;height: 80px;border: 1px solid #ccc;border-radius: 3px;text-align: center;float: left;margin: 0px 20px 40px 20px;cursor:pointer;}
.medal-list li span{display:block;}
.medal-list li img{margin-top:10px;}
.medal-list li.selected{border:1px solid #84b542;background:url({$RESOURCE_URL}select-bottom.png) no-repeat right bottom;}
.m2o-aside .m2o-item{display:-webkit-box;}
.m2o-aside .m2o-item .uc_status{display:block;height:28px;line-height:28px;}
.m2o-aside .m2o-item .member_type{display:-webkit-box;height:28px;line-height:28px;}
.m2o-aside .member_type input[type="radio"]{display:block!important;margin: 3px 5px 0px 5px;}
.m2o-aside .m2o-item .indexpic{width:176px; height:176px; line-height:176px; border:1px solid #e0dcdd; margin:5px; position:relative;
	background:url({$RESOURCE_URL}tv_interact/suoyin-default.png) no-repeat scroll center 45px #fff; font-size:0;text-align:center;cursor:pointer;}
.m2o-aside .indexpic span{display:block;font-size:18px;color:#cdd0d2;margin-top: 35px;}
.m2o-aside .m2o-item .indexpic img{vertical-align:-webkit-baseline-middle;background: #fff;max-width: 160px;max-height: 160px;}
.m2o-aside .m2o-item .indexpic-suoyin,.indexpic-suoyin-current{position:absolute; left:-2px; top:-2px; width:16px; height:46px;}
.m2o-aside .m2o-item .indexpic-suoyin{background:url({$RESOURCE_URL}tv_interact/suoyintu.png) no-repeat; }
.m2o-aside .m2o-item .indexpic-suoyin-current{background:url({$RESOURCE_URL}tv_interact/suoyintu-current.png) no-repeat;}
.m2o-aside .m2o-item label{display:block;color: #9f9f9f;width:58px;height:28px;line-height:28px;}
.m2o-m .member-info{display:none;}
.m2o-m .member-info.medal-info{padding:40px;}
.w50{width:50px;}
.w80{width:80px;}
.w100{width:100px;}
.w150{width:150px;}
.w200{width:200px;}
.m2o-m .score-info{padding:20px;}
.m2o-m .score-box{border: 1px solid #ccc;border-radius: 3px;overflow: hidden;margin: 0 auto;}
.m2o-m .score-each{height: 40px;padding: 0px 15px;}
.m2o-m .score-each img{width: 48px;height: 30px;vertical-align: middle;}
.m2o-m .score-title{background: #f9f9f9;border-bottom: 1px solid #ccc;}
.m2o-m .score-bottom{background: #f9f9f9;border-top: 1px solid #ccc;}
.m2o-m .score-list .score-each:not(:last-child){border-bottom:1px solid #ccc;}
.m2o-m .score-list .score-each:hover{background: #eee;}
.m2o-m .score-each .numbers-box{display:none;}
.m2o-m .hoge_page span{cursor:pointer;}
.m2o-m .no-data{text-align: center;color: #cd0000;line-height: 40px;}
.m2o-m .nav{border-bottom: 1px solid #ccc;padding: 15px 0;}
.m2o-m .m2o-item {font-size: 14px;padding: 10px 0px;display: -webkit-box;}
.m2o-m .nav a{width: 126px;display: inline-block;height: 34px;line-height: 34px;text-align: center;font-size: 14px;cursor: pointer;}
.m2o-m .nav a.active{background-color: #5394e4;color: #fff;}
.m2o-m .cut-off {border-bottom: 1px dotted #ccc;}
.m2o-m .m2o-item .down_list{border: 1px solid #cfcfcf!important;height: 28px!important;display:block;}
.down_list .input_left, .input_right {display: none;}
.down_list .input_middle {border: none;height: 28px!important;line-height: 28px!important;}
.down_list ul {top: 29px!important;width: 101%!important;left: -1px!important;overflow-x: hidden;}
.down_list ul li {height: 28px;}
.down_list .input_middle em {margin: 12px 4px 0 0!important;}
.m2o-m .m2o-item .title {display: inline-block;text-align: right;width: 100px;margin-right: 15px;color: #333;min-height: 24px;font-size: 14px;vertical-align: top;padding-top: 5px;}
.m2o-m .m2o-item .info{display:-webkit-box;}
.m2o-m .m2o-item input[type="text"] , .m2o-m .m2o-item input[type="password"] , .m2o-m .m2o-item input[type="email"]{width:200px;border-radius: 2px;}
.m2o-m .m2o-item .tip{display: block;color:red;margin-left: 50px;font-size:12px;margin-top: 5px;}
.m2o-m .m2o-item input[type="checkbox"]{vertical-align: sub;margin-right: 10px;}
.m2o-m .m2o-item input[type="radio"]{display:block;margin: 3px 10px 0px 0px;}
.m2o-m .m2o-item .radio-choose{display:block;margin:4px 10px 0px 0px;}
input[disabled="disabled"]{cursor: not-allowed;}
.m2o-item input[disabled="disabled"]{background:#f9f9f9;}
.m2o-item input[disabled="disabled"]:hover{border:1px solid #cfcfcf;}
.m2o-item .important{color:red;}
.m2o-item .img-box{display: -webkit-box;}
.m2o-item .img-box img{width:50px;height:50px;margin-right: 10px;}
.m2o-item .img-upload-btn{width:50px;height:50px;border:1px solid #ccc;text-align: center;font-size:30px;color:#ccc;border-radius: 2px;cursor:pointer;}
.m2o-m .bind-info{padding: 20px 35px;}
.m2o-m .bind-info li{width: 260px;height: 175px;border: 1px solid #84b542;border-radius:3px;float:left;margin: 0px 18px 35px 18px;position: relative;}
.m2o-m .bind-info .unbind{display:none;width: 55px;height: 20px;background: #84b542;position: absolute;right: 2px;top: 2px;color: #fff;font-size: 12px;text-align: center;line-height: 20px;border-top-right-radius: 3px;cursor:pointer;}
.m2o-m .bind-info li:hover .unbind{display:block;}
.m2o-m .bind-info .per-info{width:100px;}
.m2o-m .bind-info .info-list{display: -webkit-box;margin-bottom:5px;position:relative;}
.m2o-m .bind-info label{color:#868686;margin-right: 10px;}
.m2o-m .bind-info img{width:80px;height:80px;border:1px solid #ddd;margin:10px 10px 0px 10px;border-radius:3px;}
.m2o-m .per-info .info-list{padding-left: 15px;}
.m2o-m .other-info{padding:10px 10px 10px 0px;}
.m2o-m .bind-info p{overflow:hidden;white-space: nowrap;text-overflow: ellipsis;}
.m2o-m .other-info p{width:80px;}
.m2o-m .per-info p{text-align: center;padding: 0px 15px 0px 15px;}
.clone{display: block;width: 30px;height: 20px;line-height:20px;text-align:center;background:#000;color:#fff;border-radius:3px;border: 1px solid #ddd;cursor:pointer;position: absolute;right: -5px;top: 10px;display:none;}
.clone:after{display: block;top: -12px;left: 8px;position: absolute;content: '';border: 6px solid transparent;border-bottom-color: #000;}
.m2o-m .bind-info .info-list:hover .clone{display:block;}
.m2o-m .info-list object{visibility: visible;position: absolute;right: -15px;top: 6px;opacity: 0;}