{code}switch ($v['msgType']){	case 'text':		$content = $v['brief'];	break;	case 'image':		$content = '<img src="'.hg_bulid_img($v['picInfo'],35,35).'" height="35" />';	break;}{/code}<li class="common-list-data clear" id="r_{$v['id']}" name="{$v['id']}"><!--li: orderid="{$v['order_id']}"  onmouseout="hg_row_interactive(this, 'out');" onmouseover="hg_row_interactive(this, 'on');" onclick="hg_row_interactive(this, 'click', 'cur');" cname="{$v['cid']}" corderid="{$v['order_id']}"--> 	<div class="common-list-left">		<div class="common-list-item paixu">			<a class="lb" onclick="hg_row_interactive('#r_{$v['id']}', 'click', 'cur');" name="alist[]" >			<input type="checkbox" name="infolist[]"  value="{$v[$primary_key]}" title="{$v[$primary_key]}" />			</a>		</div>	</div>	<div class="common-list-right">		<span class="rr_1" id="rr_1_{$v['id']}">		<div class="common-list-item wd100">{$v['toUserName']}</div>		<div class="common-list-item wd80">{$_configs['wx_type'][$v['msgType']]['typeText']}</div>		<div class="common-list-item wd80">{$content}</div>		<div class="common-list-item wd120">{code}echo date('Y-m-d H:i:s', $v['createTime']);{/code}</div>		<div class="common-list-item wd60">{if $v['funcFlag']}★{else}☆{/if}</div>		</span>	</div>	<div class="common-list-biaoti">		<div class="common-list-item biaoti-transition">			<a href="javascript:void(0);"><span>{$v['fromUserName']}</span></a>		</div>	</div></li>