{template:./head_member}{css:manage}<style>#wrap{background:white;border-radius:3px}.nav {height: 30px; margin: 10px 0px 0px;padding-left:10px;padding-top:8px}.nav li {color: #666666;display: inline; float: left; font-size: 14px;line-height: 24px;margin-right: 20px;}.nav #goto_index {float: right;}.nav_con {line-height:20px;}.nav_con li{padding:0 0 0 20px}.nav_con input{border:1px solid #DDD;padding:3px 5px;}</style><div id="wrap" class="wrap">	{template:./top}	<ul class="nav">		<li><a href="member.php?a=edit&uid={$uid}">{$owner}的资料</a></li>		<li class="currentSetting">{$owner}的兴趣</li>		<li id="goto_index"><a href="member.php?uid={$uid}">返回{$owner}的主页</a></li>	</ul>	<div class="nav_con">		<form action="member.php?a=doeditsign" method="post" enctype="multipart/form-data" id="actionForm">			<ul>				{if $signs}					{foreach $signs as $k=>$v}					<li><strong><input type="text" value=					"{$v['mark_name']}" name="sign[]" {if $owner !="我"} disabled='disabled'{/if}/></strong></li>					{/foreach}				{else}					{$owner}没有任何兴趣				{/if}				{if $owner =="我"}<li>新增<strong><input type="text" value=				"" name="sign[]" /></strong></li>{/if}			</ul>			{if $owner =="我"}<div class="add_group_btn"><input type="submit" name="add_group_btn" value="编辑兴趣" /></div>{/if}		</form>	</div></div>{template:./footer}