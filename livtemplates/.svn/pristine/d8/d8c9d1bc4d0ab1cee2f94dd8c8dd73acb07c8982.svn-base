<table width="100%" border="0" cellspacing="0" cellpadding="0" class="columnList">	<thead>		<tr>			<th width="5%">ID</th>			<th>头像</th>			<th>用户名称</th>			<th width="25%">创建时间</th>			<th width="15%">操作</th>		</tr>	</thead>	{if $formdata && is_array($formdata)}	<tbody>		{foreach $formdata as $v}		<tr id="user_{$v['id']}">			<td><input type="checkbox" name="user_id" value="{$v['id']}" /></td>			<td>{if $v['avatar']}<img src="{code}echo hg_bulid_img($v['avatar'], 50, 50);{/code}" width="50" height="50" />{else}&nbsp;{/if}</td>			<td>{$v['user_name']}</td>			<td>{code}echo date('Y-m-d H:i:s', $v['create_time']);{/code}</td>			<td><span uri="?mid={$_INPUT['mid']}&a=userform&uid={$v['id']}&cid={$_INPUT['cid']}" class="columnBtn updateUser">编辑</span>&nbsp;|&nbsp;<span uri="?mid={$_INPUT['mid']}&a=deleteUser&id={$v['id']}" class="columnBtn deleteUser">删除</span></td>		</tr>		{/foreach}	</tbody>	{/if}	<tfoot>		<tr>			<td><input type="checkbox" name="checkall" /></td>			<td colspan="3" style="text-align:left;"><span class="columnBtn batchDelUser">删除</span>&nbsp;|&nbsp;<span uri="?mid={$_INPUT['mid']}&a=userform&cid={$_INPUT['cid']}" class="columnBtn addUser">添加</span></td>		</tr>	</tfoot></table>