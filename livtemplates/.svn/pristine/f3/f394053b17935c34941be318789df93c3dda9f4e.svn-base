<table width="100%" border="0" cellspacing="0" cellpadding="0" class="columnList">	<thead>		<tr>			<th width="5%">ID</th>			<th>栏目名称</th>			<th width="20%">创建者</th>			<th width="25%">创建时间</th>			<th width="15%">操作</th>		</tr>	</thead>	{if $formdata && is_array($formdata)}	<tbody>		{foreach $formdata as $v}		<tr id="column_{$v['id']}">			<td><input type="checkbox" name="column_id" value="{$v['id']}" /></td>			<td>{$v['name']}</td>			<td>{$v['user_name']}</td>			<td>{code}echo date('Y-m-d H:i:s', $v['create_time']);{/code}</td>			<td><span uri="?mid={$_INPUT['mid']}&a=columnform&id={$v['id']}&sid={$v['site_id']}" class="columnBtn updateColumn">编辑</span>&nbsp;|&nbsp;<span uri="?mid={$_INPUT['mid']}&a=deleteColumn&id={$v['id']}" class="columnBtn deleteColumn">删除</span></td>		</tr>		{/foreach}	</tbody>	{/if}	<tfoot>		<tr>			<td><input type="checkbox" name="checkall" /></td>			<td colspan="3" style="text-align:left;"><span class="columnBtn batchDelColumn">删除</span>&nbsp;|&nbsp;<span uri="?mid={$_INPUT['mid']}&a=columnform&sid={$_INPUT['sid']}" class="columnBtn addColumn">添加</span></td>		</tr>	</tfoot></table>