.table-box{margin:20px 10px; }
.table-box .tdCheck{text-align:center; }
.table-box caption{font-size:16px; color:#333; text-align: center; }
.table-box table{font-size:14px; }
.table-box thead{font-weight:bold;  }
.table-box .footer{padding:10px 18px; }
.table-box .footer label{font-size:12px; color:#999; }
.table-box .colspan{line-height:34px; text-align:center; color:#da2d2d;}
.table-box .next{text-align:center; }

.nav-btn{position: fixed; top:10px; right:10px; text-align:center; }
.btn-wid80{display:inline-block; width:80px; height:34px; line-height: 34px; padding:0; border-radius:2px; color:#fff; }
.btn-wid80:hover{color:#fff; opacity: 0.8; }
.btn-print{background-color:#649ee8; }
.btn-set{background-color:#4c4c4c; }

.m2o-flex{display:-webkit-box; display:-moz-box; display:-ms-flexbox; width:100%; }
.m2o-flex-center{-webkit-box-align:center; -moz-box-align:center; -ms-flex-align: center;}
.m2o-flex-end{-webkit-box-align:end; -moz-box-align:end; -ms-box-align:end;box-align:end;}
.m2o-flex-start{-webkit-box-align:start; -moz-box-align:start; -ms-box-align:end;box-align:start;}
.m2o-flex-one{-webkit-box-flex:1; -moz-box-flex:1; -ms-flex:1;}

.edit{width:480px; margin:54px 20px; overflow:hidden; -webkit-transition: width 0.3s; transition: width 0.3s; }
.edit.fold{width:0; }
textarea#source{width:100%; min-height:400px; color:#48484c; background-color:#f7f7f9; border:1px solid #e1e1e8; border-radius:4px; resize:vertical; }
textarea#source:focus{outline: none; }
