body{padding: 10px;}
.result-info-box .info h1{color:#da2d2d;text-align: center;}
.title p{font-size: 20px;color: #999;text-indent: 25px;height: 40px;border-bottom: 1px dotted #999;line-height: 40px;}
.title .total-num{float: right;margin: -30px 10px;font-size: 14px;color: #5C99CF;}
.title em{color:#cd0000;margin: 0 5px 0 5px;}
.info-list{margin: 9px 30px -5px;overflow: hidden;}
.info-list li {float: left;position: relative;width: 235px;overflow: hidden;margin: 10px 30px 10px;border: 2px solid #eaeaea;padding: 15px;border-radius: 5px;min-height: 115px;}
.img {width: 60px;text-align: center;float : left}
.img img {width: 60px;height: 60px;display: block;border: 1px solid #eaeaea;}
.img .logo-name{color:#999}
.img span {display: block;text-align: center;line-height: 24px;margin-top: 5px;cursor: pointer;}
.list-profile {margin-left: 15px;}
.info-intro:first-child {margin-top: 0;}
.info-intro {border-bottom: 1px dotted #e1e1e1;line-height: 25px;font-size: 13px;margin-top: 4px;margin-left: 55px;width: 173px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.info-intro label{color: #999;padding-left: 5px;}
.info-intro span{color:#282828}
.info-time{color: #999;margin-top: 10px;}
.page_size select{display:none}