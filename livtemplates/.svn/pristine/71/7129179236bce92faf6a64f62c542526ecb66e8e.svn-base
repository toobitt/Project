 @import 'http://localhost/livtemplates/tpl/lib/less/common_less.less';
 /*ftp大文件提交*/
 .path_show{width:91%; height:@m10*3; border:1px solid lighten(@color-gray,40%); margin-left:@w43+@m10/2; margin-top:@m10/2 - 1px; clear:both;
 	border-bottom: 1px dashed lighten(@color-gray,20%);}
 .dircss{line-height:@m10*3; cursor:pointer; width:@m10*8 + @m10/2 - 1px; height:@w100 + @m10 + 2px; float:left; margin-left:@w43 + 1px; margin-top:@m10 + @m10/2; position:relative; text-align: center;
        span{width:@m10*8 + 4px; display:block; .ellipsis()}
 }
 .folder{background:url("@{path}folder.png"); width:100%; height:60%; border:1px solid lighten(@color-gray,40%);
       &:hover{background:url("@{path}folder_active.png"); width:100%; height:60%; border:1px solid lighten(@color-gray,40%);}
 }
 .file_png{background:url("@{path}file.png"); width:100%; height:60%;
       &:hover{background:url("@{path}file_active.png"); width:100%; height:60%;}
 }
 .navcss{border-left: 1px solid lighten(@color-gray,40%); color:darken(@color-gray,40%) - #101010; font-size: @font-size12 + 2px; margin-left: @m10/2 - 1px;
      span{color:lighten(@color-gray,20%);}
      &:hover{color:@color-c8d4e2 + #071015;}
 }
 .select{background:url("@{path}select.png"); width:@m10*2 + 1px; height:@m10*2 + 1px; .position-lefttop(6px,-10px); display:none;}
 .is_submit{background:url("@{path}is_submit_1.png"); width:@m10 + @m10/2 + 1px; height:@m10*4 + @m10/2; .position-lefttop(50px , 0);}
 .file input{float:left; margin-top:@m10 + 1px; display:none}
 .current{opacity:1}
 .pic{background:url("@{path}file-none.png");
      &:hover{background:url("@{path}file-none.png");}
 }
 .result-tip{overflow:hidden; z-index:-1; .position-lefttop(50% , 65px); font-size:@m10*2 + 4px; background:lighten(@color-gray,40%);text-align:center;
 	margin-left:@z-1*@w100*2; .rounded-corners(4px); width:@w100*3 + @m10*5; height:@m10*6; border:4px solid #6ba4eb; .box-shadow-custom(0,0,4px,0, 0.5);
 	line-height:@m10*6; .transition(all,3s, liear); opacity:0;}
 .file_size{width: @m10*8 + 2px; height: @m10*2 + @m10/2 + 1px; .position-lefttop(0 ,74px); background:@color-aed2f2; .rounded-corners(2px); display:none;
      span{width:@m10*8 + 2px; height:@m10*2 + @m10/2 + 1px;}
      em{border:6px solid @color-aed2f2; border-color: transparent transparent @color-aed2f2; .position-lefttop(33px, -12px);}
 }
 
 
 
