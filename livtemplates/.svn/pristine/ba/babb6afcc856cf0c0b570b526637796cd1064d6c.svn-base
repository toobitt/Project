.nav-box{display:block;}
.m2o-form{background:white;}
.wrap{padding-bottom:40px;}
.epaper-info{padding:20px;border-right:1px solid #ccc;height:600px;}
.info-item{padding:15px 0;height:10px;}
1.info-item:hover{background:#f2f2f2;}
.info-item .title{color:#9f9f9f;display:inline-block;width:65px;}
.info-item input{border-color:transparent;}
.info-item input:focus{box-shadow:none;border-color:#cfcfcf;}
.info-item .photo{display:inline-block;width:100px;height:100px;background:#b31826;}
.info-item .photo img{max-width:200px;min-width: 100px;height:100%;}
.save-button {margin: 20px 0;}
.saveBtn:hover{border:none;}
.m2o-item{padding:0;border:0;}
.m2o-item:hover{background:none;}
.epaper-list{overflow:hidden;}
.m2o-item.num{width:70px;}
.m2o-item.date{width:100px;}
.info-item .down_list{margin-left: 70px;margin-top: -20px;cursor:pointer;}
.info-item .down_list ul{top:24px;left:0px;}
.info-item .down_list ul li{height:24px;line-height:24px;}
.current{background-color:#EEEFF1;}
.result-tip{overflow:hidden;z-index:-1;position:absolute;top:215px;left:17%;font-size:25px;background:#fff;text-align:center;margin-left:-200px;border-radius:4px;width:218px;height:60px;border:4px solid #6ba4eb;box-shadow:0 0 4px #ccc;line-height:60px;transition:all 3s;-webkit-transition:all 3s;-moz-transition:all 3s;opacity:0;}
#popup_panel #popup_ok{cursor:pointer}
#popup_panel #popup_cancel{cursor:pointer}
.confim-sure{cursor:pointer;}
.confim-cancel{cursor:pointer;}
#top-loading{position:absolute;top:225px;left:85px;width:70px;height:70px;}
.hoge_page{margin-right:50px}