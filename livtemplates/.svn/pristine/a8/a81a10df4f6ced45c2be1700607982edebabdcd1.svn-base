{code}$needUpload = $owner && ($owner == '我') && $photos && ($photos['sys'] == 0);{/code}<ul class="scrollUl fz">	{if $needUpload}	<li class="t01" id="upload_albums_btn" albumsid="{$photos['albums_id']}">上传图片</li>	{/if}	<li class="t01" id="back_albums_btn">返回相册</li>	<script type="text/javascript">	$('#back_albums_btn').click(function () { 		$('#tabNav li[m=photo]').addClass('fei').trigger('click'); 	});	</script></ul><hr class="hr_lv">{if $photos && $photos['photos']}<ul id="pic_list">{if $photos['photos']}	{foreach $photos['photos'] as $v}	<li id="photo_{$v['p_id']}">		<p><img src="{$v['photo_info']['host']}126x126/{$v['photo_info']['filepath']}" width="126" height="126" /></p>		{if ($_user['id'] == $photos['user_id']) && ($photos['sys'] != 1)}		<p><a href="javascript:;" m="drop_photo" pid="{$v['p_id']}" albumsid="{$photos['albums_id']}">删除</a></p>		{/if}	</li>	{/foreach}{/if}</ul>{if $more_imgs}<div class="gtalk_more moreCon" style="clear:both;" total="{$p_count}" albumsid="{$photos['albums_id']}">点击获取更多信息...</div>{/if}<style><!--#pic_list li {float:left; margin:10px;}--></style><script type="text/javascript">$(function() {	var page = 2;	$('.moreCon').click(function() {		var _this = $(this);		var _v = $(this).html();		var total = _this.attr('total');		var albumsid = _this.attr('albumsid');		$.ajax({			type : 'post',			url : 'member.php',			data : 'a=get_albums_imgs&uid='+uid+'&aid='+albumsid+'&total='+total+'&p='+page+'&ajax=1',			cache : false,			async : true,			dataType : 'json',			success : function(data) {				var result = eval(data.callback);				if (page < result.totalPages)				{					page++;					$('#pic_list').append(result.html);					_this.html(_v);				}				else				{					$('#pic_list').append(result.html);					_this.remove();				}			},			beforeSend : function() {				_this.html('<br /><img src="'+RESOURCE_URL+'loading.gif" />');			}		});	});		function hg_call_images(data, total)	{		return {			html : data,			totalPages : total		};	}	$('#pic_list a').click(function() {		var method = $(this).attr('m');		var pid = $(this).attr('pid');		var albumsid = $(this).attr('albumsid');		$.getJSON('member.php?a='+method+'&pid='+pid+'&albumsid='+albumsid, function(data) {			if (data.msg) {				$('#photo_'+pid).remove();			}else {				alert(data.error);			}		});	});	$("#upload_albums_btn").click(function() {		// 上传方法		var albums_id = $(this).attr('albumsid');		$.upload({			// 上传地址			url: 'member.php', 			// 文件域名字			fileName: 'Filedata', 			// 其他表单数据			params: {a: 'create_photo', albumsid: albums_id},			// 上传完成后, 返回json, text			dataType: 'json',			// 上传之前回调,return true表示可继续上传			onSend: function() {				return true;			},			// 上传之后回调			onComplate: function(data) {				if (data.error)				{					alert(data.error);				}				else				{					var html = '<li id="photo_'+data.pid+'"><p><img src="'+data.host+'126x126/'+data.filepath+'" width="126" height="126" /></p>';					if (data.uid == gAdmin.admin_id)					{						html += '<p><a href="javascript:;" m="drop_photo" pid="'+data.pid+'">删除</a></p>';					}					html += '</li>';					$('#pic_list').append(html);				}			}		});	});});</script>{else}<p>暂没有信息</p>{/if}