.wrap{background:#f1f1f1;padding:10px 10px 10px 10px;}
.wrap .total-wrap-chart{overflow: hidden;margin-bottom: 10px;}
.wrap .total-wrap-chart>*{background:#fff;border:1px solid #e9e9e9;float:left;}
.wrap .line-chart{width:395px;height:210px;padding:10px;margin:0px 10px 0px 0px;}
.wrap .bar-chart{width:415px;height:210px;padding:10px;margin:0px 10px 0px 0px;}
.wrap .pie-chart{width:292px;height:210px;padding:10px;position:relative;}
.wrap .pie-chart canvas{margin: 20px 10px 10px 10px;}

.wrap .pie-chart .pie-all{display: block;width: 50px;height: 50px;position: absolute;text-align: center;font-size: 14px;left: 58px;top: 109px;display:none;}
.wrap .pie-chart .pie-all>*{display:block;}

.wrap .pie-chart .pie-info-list{position: absolute;top: 70px;right: 0px;}
.wrap .pie-info-list li{margin-top:5px;}
.wrap .pie-info-list li >*{display:block;}
.wrap .pie-chart .pie-info-color{width: 15px;height: 15px;background: #a9d86e;border-radius: 2px;}
.wrap .pie-chart .pie-info-title{width: 60px;margin: 0px 10px 0px 10px;}
.wrap .pie-chart .pie-info-num{width: 50px;color: #8fc1d8;}

.wrap .department-wrap{padding: 10px 15px 5px 15px;background: #fff;border: 1px solid #e0e0e0;min-height: 390px;}
.wrap .department-wrap .department-list{overflow:hidden;}
.wrap .department-wrap-nav{height:45px;position:relative;}
.wrap .nav-list span{float:left;color:#999;font-size:14px;cursor:pointer;height: 45px;line-height: 40px;}
.wrap .nav-list span.active{color:#2b84e0;}
.wrap .nav-list span.divide-line{margin:-1px 5px;}





.wrap .department-list li{width:221px;height:165px;background: #fff;float: left;border:1px solid #e9e9e9;margin-bottom:10px;}
.wrap .department-list li:not(:nth-of-type(5n)){margin-right:10px;}
.wrap .department-list .chart-area-bar{display:none;} 
1.wrap .department-list .department-chart{width:197px;height:140px;padding:15px;}
.wrap .department-list .check-more{display:block;width:100%;height:36px;line-height:36px;text-align:center;background:#f9f9f9;border-top:1px solid #e9e9e9;color:#aeaeae;text-decoration:underline;cursor:pointer;}
.wrap .department-list .more-detail{position: absolute;right: 5px;top: 8px;color: #999;text-decoration: underline;}
.wrap .department-chart{height:30px;position: relative;margin-bottom: 10px;}
.wrap .department-chart .chart-title{display:block;height:100%;line-height:35px;font-size:14px;}
.wrap .search-box{display: -webkit-box;position: absolute;right: 0px;top: 3px;z-index: 99;}
.wrap .search-item{margin-left:5px;position: relative;}
.wrap .search-item .search-item-title{display:block;width: 60px;padding:4px 20px 4px 4px;border: 1px solid #ccc;border-radius:2px;position: relative;background: #fff;cursor:pointer;}
.wrap .search-item .search-item-title:after{display: block;content: '';border-top: 6px solid #000;border-right: 5px solid transparent;border-left: 5px solid transparent;position: absolute;right: 5px;top: 10px;}
.wrap .search-item .type-list{border: 1px solid #e0e0e0;border-radius: 2px;background: #fff;z-index: 9999;}
.wrap .type-list li{height:28px;line-height:28px;padding-left:4px;padding-right:4px;cursor:pointer;}
.wrap .type-list li:not(:last-child){border-bottom:1px solid #e0e0e0;}
.wrap .type-list li:hover{background-color:#e0e0e0;}
.wrap .type-list input[type="checkbox"] , .wrap .type-list input[type="radio"]{vertical-align: middle;margin: -3px 5px 0px 3px;}

.search-time-box{position: absolute;width: 215px;border: 1px solid #e0e0e0;right: 0px;top: 35px;background: #fff;z-index: 9999;border-radius: 3px;padding: 5px;}
.search-time-box:before{display: block;content: '';border-bottom: 6px solid #d9dfe7;border-right: 5px solid transparent;border-left: 5px solid transparent;position: absolute;right: 5px;top: -7px;}
.search-time-box:after{display: block;content: '';border-bottom: 6px solid #fff;border-right: 5px solid transparent;border-left: 6px solid transparent;position: absolute;right: 5px;top: -6px;}
.search-time-box ul{overflow:hidden;padding-bottom: 6px;border-bottom: 1px solid #e0e0e0;}
.search-time-box ul li{float: left;width: 50px;height: 25px;background: rgba(0,0,0,.5);color:#fff;text-align: center;line-height: 25px;border-radius: 2px;cursor:pointer;}
.search-time-box ul li.current , .search-time-box ul li:hover{background:#2b84e0;}
.search-time-box ul li:not(:last-child){margin: 0px 5px 0px 0px;}
.search-time-box input{width:92px;height:19px;border-radius:2px;}
.search-time-box .custom-tip{display: block;height: 25px;line-height: 25px;}
.search-time-box .custom{display: -webkit-box;}

.wrap .department-list .chart-title-wrap{width:197px;height:140px;padding: 5px 10px 0px 10px;}

.wrap .department-list .department-chart{padding:0px 10px;}
.wrap .toggle-chart{padding: 0!important;width:52px;height:26px;background:#fff;border:1px solid #e9e9e9;display: -webkit-box;border-radius: 2px;margin-left: 5px;}
.wrap .toggle-chart>*{display:block;width:26px;height:100%;background-color:#ededed;cursor:pointer;background-size: 16px;background-repeat: no-repeat;background-position: center center;}
1.wrap .toggle-chart span:first-child{border-right:1px solid #e9e9e9;}
.wrap .toggle-chart span.current{background-color:#fff;}
.wrap .department-list li.more-department{width:201px;height:145px;padding:10px;}
.wrap .department-list li.more-department span{display:block;width:100%;height:100%;line-height:145px;text-align:center;background:#f9f9f9;color:#aeaeae;text-decoration: underline;cursor:pointer;}
.wrap .toggle-chart span.line{background-image:url({$RESOURCE_URL}workload/zhexiantu_normal-2x.png)}
.wrap .toggle-chart span.current.line{background-image:url({$RESOURCE_URL}workload/zhexiantu_active-2x.png)}
.wrap .toggle-chart span.bar{background-image:url({$RESOURCE_URL}workload/shuxiantu_normal-2x.png)}
.wrap .toggle-chart span.current.bar{background-image:url({$RESOURCE_URL}workload/shuxiantu_active-2x.png)}

/*各部门*/
.wrap .left-aside{width:520px;}
.wrap .title{height: 35px;font-size: 14px;line-height: 35px;margin-bottom: 10px;}
.wrap .title span{font-weight: bold;}
.wrap .left-aside .list-pie-info{width:145px;height:190px;background:#fff;padding:20px;border:1px solid #e0e0e0;margin-right: 10px;}
.wrap .list-pie-info li>*{float:left;display:block;font-size: 12px;}
.wrap .list-pie-info li span:first-child{width:112px;height:34px;}
.wrap .list-pie-info li span:last-child{color:#8dbfd8;}
.wrap .pie-chart , .wrap .line-chart , .wrap .advance-person{background:#fff;border:1px solid #e0e0e0;}
.wrap .left-aside .line-chart{width:467px;height:210px;margin-top: 10px;padding:10px 20px;}
.wrap .left-aside .advance-person{width:467px;height:110px;margin:10px 0px 0px 0px;padding: 7px 20px 13px 20px;}
.wrap .left-aside .top10{display: block;height: 35px;line-height: 35px;font-size: 14px;}
.wrap .top10-list li{float: left;width: 91px;height: 35px;text-align: center;line-height: 35px;font-size: 12px;color: #323232;border: 1px solid #e0e0e0;background: #f9f9f9;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.wrap .content-wrap .info-list{background:#fff;border:1px solid #e0e0e0;}
.wrap .info-list .search-box{right: 10px!important;top: 15px!important;}
.wrap .info-list .m2o-title , .wrap .info-list .info-list-bottom{background: #f9f9f9;padding: 0 20px;color:#999;min-height:41px;}
.wrap .info-list .info-list-bottom{border-top:1px solid #c8d4e0;}
.wrap .info-list .list-type,.wrap .info-list .list-view ,.wrap .info-list .list-column{width:80px;font-size:12px;}
.wrap .info-list .list-time{width:135px;font-size:12px;}
.wrap .info-list .list-column a{text-decoration: underline;color: #49a34b;}
.wrap .info-list .list-video , .wrap .info-list .list-pic , .wrap .info-list .list-news , .wrap .info-list .list-back ,.wrap .info-list .list-publish{width:50px;color:#999;}
.wrap .info-list .list-audit , .wrap .info-list .list-publish , .wrap .info-list .list-top{width:70px;color:#999;}
.wrap .info-each .list-publish{color:#8dbfd8;}
.wrap .info-list .info-each-list{margin: 0 10px;position: relative;height: 532px;}
.wrap .info-list .info-each{padding: 0 10px;}
.wrap .info-list .info-each{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;width: 100%;position: relative;border-bottom: 1px solid #c8d4e0;min-height: 41px;}
.wrap .info-list .info-each.no-data{text-align: center;line-height: 40px;color: #da2d2d;}
.wrap .info-list .list-title{white-space: nowrap;overflow: hidden;text-overflow: ellipsis;padding-right: 10px;}
.wrap .list-title .list-img{width:30px;height:30px;border-radius:50%;vertical-align: middle;}
.wrap .list-title .list-indexpic{width:40px;height:30px;vertical-align: middle;}
.wrap .info-list .list-time , .wrap .info-list .list-view{color:#999;}
.wrap .info-each-list .list-type{color:#8dbfd8;}
.wrap .info-each:hover{background:#f8f8f8;}
.wrap .info-each:hover .list-title{padding-left: 10px;transition: all 0.5s;}

.wrap .numbers-box{display:none;}