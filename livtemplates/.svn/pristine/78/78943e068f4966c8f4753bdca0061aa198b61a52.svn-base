.wrap{border:0;background:transparent;box-shadow:none;}
.items{margin:20px 0 20px 35px;}
.wrap>.items{margin-right:35px;}
.name-box{padding-right:15px;font-size:20px;line-height:28px;display:inline-block;background:#fff;}
.option-box{font-size:12px;display:none;line-height:60px;}
.option-box div{position:relative;margin-right:30px;font-size:14px;color:#6aa5eb;padding-left:20px;float:left;}
.option-box .item-child-toggle:before{position:absolute;left:0;top:50%;margin-top:-9px;content:'';width:15px;height:15px;background:url({$RESOURCE_URL}auth/auth_open.png) no-repeat}
.option-box .item-child-toggle-up:before{background-image:url({$RESOURCE_URL}auth/auth_close.png);}

.members{1border-bottom:1px solid #d8d8d8;1padding-bottom:35px;}
.members-active{opacity:.5;background:#ccc;}
.members-hover{opacity:.5;background:#000;}
.member, .member-add{float:left;margin:10px 15px;text-align:center;}
.member-avatar{height:90px;width:90px;border:3px solid #e5e5e5;border-radius:50%;display:block;overflow:hidden;}
.member-avatar:hover, .member.on .member-avatar, .member-add.on .member-avatar{border-color:#6aa5eb;box-shadow:0 0 10px #b8dbfb;-webkit-transition:all .2s ease-in .2s;-moz-transition:all .2s ease-in .2s;-ms-transition:all .2s ease-in .2s;transition:all .2s ease-in .2s;}
.member-avatar img{max-height:90px;max-width:90px;}
.member-admin .member-avatar, .member-admin.on .member-avatar{border-color:#ffaa00;}
.member-admin .member-avatar:hover, .member-admin.on .member-avatar{box-shadow:0 0 10px #faa;}
.member-name{display:block;margin-top:10px;height:18px;line-height:18px;}
.member-add{cursor:pointer;font-size:30px;font-weight:bold;line-height:90px;}
.member-add .member-avatar{background:url({$RESOURCE_URL}auth/auth_bigadd.png) no-repeat center #e5e5e5;}

.item-hd-edit form{float:left;}
.delete{float:right;background:transparent;margin-right:20px;color:red;}

.item-child{display:none;}
