input[type="radio"]{display:inline-block!important;vertical-align: sub;}
.m2o-item{color: #9f9f9f;}
.m2o-item #top-loading{position: absolute;left: 50%;top: 70px;width: 50px;margin-left: -25px;z-index: 10000;display:none;}
.m2o-item span{margin-left: 5px;}
.m2o-item input[type="checkbox"] {height: 13px;margin-right: 5px;}
.m2o-aside{width:300px}
.m2o-aside .m2o-right{margin: -22px 0 0 95px;}
.m2o-aside .item-display{font-size: 17px;color: #000;}
.m2o-aside .item-show{width:275px;height:80px;background:#fff;border:1px solid #e7e7e7;display: table-cell;vertical-align: middle;text-align: center;}
.code-info li{border-bottom:1px solid #e7e7e7;height: 40px;padding: 20px;width: 95%;}
.code-info li:last-child{border-bottom:none;}
.code-info .m110{margin-left:110px;}
.code-info .m10{margin-top:10px;}
.code-info .list-title{text-align: left;}
.code-info .title{font-size:15px;}
.code-info .notice{color:#9f9f9f;}
.code-info .select-input{}
.code-info .text-input{margin:0 5px 0 5px;width:40px;}
.code-info .list-info{margin: 4px 0 0 40px;}
.code-info .list-info input[type="checkbox"]{vertical-align: sub;}
.code-info .list-set{margin: -2px 0 0 15px;}
.code-info .m20{margin-left:20px;vertical-align: sub;}
.code-info .count-prevent{width:100%;height:100%;position:absolute;top: 0px;left: 0px;background:rgba(249,249,249,0.6);z-index:99999;}
.code-info .count-num{display:none;}
.code-info .bg-display{display:none;}
.code-info .input-info{width:100px;margin:6px 0 0 30px;text-indent:5px;}
.code-info .select-box{margin: 6px 0 0 20px;}
.code-info .default{display: -webkit-box;position: relative;margin:6px 0 0 30px;}
.code-info .colorpicker-wrap{margin-left:5px;}
.code-info .img-info{width:130px;background: #cfcfcf;margin: 0 0 0 20px;position:absolute;display:none;}
.code-info .img-info .upload-file{width:120px;height:30px;background:#fff;display:block;margin: 5px;line-height: 30px;text-align: center;cursor:pointer;}
.code-info .upload-img{width:125px;height:35px;margin-left: 150px;}
.down_list{border: 1px solid #cfcfcf!important;height: 28px!important;margin-left: 5px;}
.down_list .input_left,.input_right{display:none;}
.down_list .input_middle{border:none;height:28px!important;line-height:28px!important;}
.down_list ul{top: 29px!important;width: 101%!important;left: -1px!important;overflow-x: hidden;border-right:1px solid #cfcfcf;border-bottom:1px solid #cfcfcf;}
.down_list ul li{height:28px;}
.down_list ul li:last-child{height:28px;border-bottom:none;}
.down_list .input_middle em{margin: 12px 4px 0 0!important;}
2.item-show img{margin: 10px;}
.preview{width: 60px;height: 25px;background: #6ba4eb;color:#fff;text-align:center;border-radius:3px;cursor:pointer;float: right;line-height: 25px;}
.m2o-item:first-child:hover .preview{display:block!important;}
.evo-pop{z-index: 999999!important;}




