{foreach $albums_info as $v}<li id="albums_{$v['albums_id']}">	<a href="javascript:;" albumsid="{$v['albums_id']}"><img src="{if is_string($v['cover_file_name'])}{$v['cover_file_name']}{else}{$v['cover_file_name']['host']}126x126/{$v['cover_file_name']['filepath']}{/if}" /></a>	<p><a href="javascript:;" albumsid="{$v['albums_id']}">{$v['albums_name']}（{$v['picture_count']}张）</a></p>	{if $_user['id'] == $v['user_id']}	<p><a href="javascript:;" m="drop_albums" albumsid="{$v['albums_id']}">删除</a></p>	{/if}</li>{/foreach}